0;\r\n    s = (prec ? toFixedFix(n, prec) : \"\" + Math.round(n)).split(\".\");\r\n    if (s[0].length > 3) {\r\n      s[0] = s[0].replace(/\\B(?=(?:\\d{3})+(?!\\d))/g, sep);\r\n    }\r\n    if ((s[1] || \"\").length < prec) {\r\n      s[1] = s[1] || \"\";\r\n      s[1] += new Array(prec - s[1].length + 1).join(\"0\");\r\n    }\r\n    return s.join(dec);\r\n  }\r\n});\r\n\r\nVue.filter(\"color_sty\", function(cls) {\r\n  let color_sty = \"\";\r\n  switch (cls) {\r\n    case \"ssr\":\r\n      color_sty = \"FF1A01\";\r\n      break;\r\n    case \"ss\":\r\n      color_sty = \"FFE107\";\r\n      break;\r\n    case \"s\":\r\n      color_sty = \"E740EB\";\r\n      break;\r\n    case \"a\":\r\n      color_sty = \"B3FCFE\";\r\n      break;\r\n    case \"b\":\r\n      color_sty = \"BDF101\";\r\n      break;\r\n\r\n    default:\r\n      color_sty = \"ffffff\";\r\n  }\r\n  return color_sty;\r\n});\r\n\r\nconst API_URL = Vue.observable( \"http://api-gamemarget.ap-southeast-1.elasticbeanstalk.com\" );\r\n\r\n\r\nVue.prototype.$API_URL = API_URL;\r\n\r\n// Vue.prototype.$actions = actions;\r\n\r\nVue.use(VueRouter);\r\nnew Vue({\r\n  router,\r\n\r\n  render: (h) => h(App),\r\n}).$mount(\"#app\");\r\n"]}]}