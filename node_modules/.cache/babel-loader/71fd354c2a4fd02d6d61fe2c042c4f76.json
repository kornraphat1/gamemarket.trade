;;AACA;AAEA,MAAA,OAAA,CAAA,GAAA,CAAA,eAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,UAAA;AACA,WAAA,QAAA,CAAA,GAAA,CAAA,WAAA,EAAA,EAAA;AAEA,UAAA,aAAA,GAAA,EAAA;AAEA,WAAA,UAAA,CAAA,MAAA;;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,UAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,QAAA,aAAA,CAAA,IAAA,CAAA,KAAA,UAAA,CAAA,CAAA,EAAA,QAAA;AACA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,aAAA;AACA,UAAA,IAAA,GAAA,IAAA,MAAA,EAAA;AAEA,MAAA,IAAA,GAAA;AACA,QAAA,eAAA,EAAA;AADA,OAAA;AAKA,UAAA,MAAA,GAAA;AACA,QAAA,OAAA,EAAA;AAAA,UAAA,aAAA,mBAAA,KAAA,QAAA,CAAA,GAAA,CAAA,YAAA,CAAA;AAAA;AADA,OAAA;AAGA,WAAA,QAAA,CAAA,qBAAA,EAAA,IAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAEA,IAFA,CAEA,MAAA,CAAA,QAAA,GAAA,uCAFA,EAEA,IAFA,EAEA,MAFA,EAGA,IAHA,CAGA,UAAA,QAAA,EAAA;AACA,cAAA,QAAA,CAAA,IAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,YAAA,QAAA,CAAA,cAAA,CAAA,SAAA,EAAA,KAAA;;AACA,YAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,kBAAA,EAAA,SAAA,EAAA,IAAA,CAAA,YAAA;AACA,cAAA,QAAA,CAAA,OAAA,CAAA,UAAA;AACA,aAFA;;AAGA,YAAA,UAAA,CAAA,YAAA;AACA,cAAA,QAAA,CAAA,OAAA,CAAA,UAAA;AACA,aAFA,EAEA,IAFA,CAAA,CANA,CASA;AACA;AACA;AACA,WAZA,MAYA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,IAAA;;AACA,YAAA,MAAA,CAAA,MAAA,CACA,gDADA,EAEA,gBAFA,EAGA,OAHA;AAKA;AACA,SAxBA,EAyBA,KAzBA,CAyBA,UAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,YAAA,GAAA,KAAA,CAAA,OAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA;AACA,UAAA,KAAA,CAAA,KAAA,CAAA;AACA,SA7BA;AA8BA,OA/BA;AAgCA;AA/IA,GA9BA;AAgLA,EAAA,QAAA,EAAA,EAhLA;AAkLA,EAAA,KAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,UAAA,KAAA,GAAA,EAAA;AACA,MAAA,KAAA,GAAA,KAAA,UAAA,CAAA,MAAA,CAAA,UAAA,KAAA,EAAA,IAAA,EAAA;AAAA;;AACA,YAAA,OAAA,GAAA,IAAA,CAAA,OAAA;AACA,QAAA,KAAA,CAAA,OAAA,CAAA,qBAAA,KAAA,CAAA,OAAA,CAAA,2DAAA,EAAA;AACA,QAAA,KAAA,CAAA,OAAA,CAAA,CAAA,IAAA,CAAA,IAAA;AACA,eAAA,KAAA;AACA,OALA,EAKA,EALA,CAAA;AAOA,WAAA,WAAA,CAAA,KAAA;AACA,KAXA,CAYA;AACA;AACA;;AAdA;AAlLA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"bg-third\" id=\"section-third\" v-if=\"pageTeade == 2\">\r\n      <div class=\"container\">\r\n        <div class=\"toBackContainer\">\r\n          <div class=\"toBack\" @click=\"goBack()\">\r\n            <i\r\n              style=\"padding-left:5px; color:#e740eb\"\r\n              class=\"fas fa-angle-left\"\r\n            ></i>\r\n            ย้อนกลับ\r\n          </div>\r\n        </div>\r\n        <div class=\"headerContainer\">\r\n          <div class=\"left\"></div>\r\n          <div class=\"right\">\r\n            <div class=\"title\">ราคาต่อชิ้น</div>\r\n            <div class=\"title\">จำนวน</div>\r\n            <div class=\"title\">ราคารวม</div>\r\n          </div>\r\n        </div>\r\n        <div class=\"bodyContainer\">\r\n          <div class=\"itemList\" v-for=\"(v, key) in ItemShow\" :key=\"key\">\r\n            <div class=\"left\">\r\n              <div class=\"imgTrade\">\r\n                <img\r\n                  :src=\"v.picture ? $IMG_URL_ITEM + v.picture : $IMG_image\"\r\n                  class=\"item-img-detail\"\r\n                />\r\n              </div>\r\n              <div class=\"deteilItem\">\r\n                <div class=\"tradeDetail-name-eng\" :style=\"'trade' | color_sty\">\r\n                  {{ v.name.en }}\r\n                </div>\r\n                <div class=\"tradeDetail-name-thai\">\r\n                  {{ v.name.th }}\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"right\">\r\n              <div class=\"title\">{{ v.amount }}</div>\r\n              <div class=\"title\">{{ v.qty }}</div>\r\n              <div class=\"title\">{{ v.allamount }}</div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"footerContainer\">\r\n          <div class=\"left\"></div>\r\n          <div class=\"right\">\r\n            <div class=\"title\"></div>\r\n            <div class=\"title\">ราคารวม(ทั้งหมด)</div>\r\n            <div class=\"title\">{{ allamount }} P</div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"buttonContainer\">\r\n          <div>\r\n            <button\r\n              class=\"btn Tradebtn\"\r\n              id=\"editProfile\"\r\n              data-toggle=\"modal\"\r\n              data-target=\"#ModalVertify\"\r\n              v-if=\"TradeStetus == false\"\r\n            >\r\n              Trade\r\n            </button>\r\n            <button\r\n              class=\"btn Tradebtn\"\r\n              @click=\"trade()\"\r\n              v-else-if=\"TradeStetus == true\"\r\n            >\r\n              Trade\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <Vertify\r\n      :wistatus_frist=\"Wistatus\"\r\n      :is_verify_otp=\"isVerifyOTP\"\r\n      :is_verify_wiwallet=\"isVerifyWiwallet\"\r\n      v-on:gotrade=\"gotrade\"\r\n    ></Vertify>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// import Bank from \"../Bank.vue\";\r\nimport Vertify from \"../Vertify.vue\";\r\nexport default {\r\n  props: {\r\n    SelectItem: {\r\n      type: Array,\r\n      //   default: [],\r\n    },\r\n    pageTeade: Number,\r\n  },\r\n  components: {\r\n    //  Bank,\r\n    Vertify,\r\n  },\r\n  data() {\r\n    return {\r\n      ItemShow: [],\r\n      allamount: 0,\r\n      isVerifyOTP: \"\",\r\n      isVerifyWiwallet: \"\",\r\n      Wistatus: \"\",\r\n      TradeStetus: false,\r\n    };\r\n  },\r\n\r\n  mounted() {\r\n    this.checkStatusWi();\r\n    this.getVerifyStatus();\r\n    if (this.SelectItem) {\r\n      this.$emit(\"toBack\", 1);\r\n    }\r\n  },\r\n  methods: {\r\n    gotrade(value) {\r\n      if (value) {\r\n        this.trade();\r\n      }\r\n    },\r\n    goBack() {\r\n      this.$emit(\"toBack\", 1);\r\n      // console.log(5);\r\n    },\r\n    setShowItem(group) {\r\n      let index = 0;\r\n      let self = this;\r\n      this.ItemShow = [];\r\n      for (let g in group) {\r\n        this.ItemShow[index] = group[g][0];\r\n\r\n        this.ItemShow[index].allamount = 0;\r\n\r\n        this.ItemShow[index].qty = group[g].length;\r\n        for (let i = 0; i < group[g].length; i++) {\r\n          this.ItemShow[index].allamount += group[g][i].amount;\r\n        }\r\n\r\n        index++;\r\n      }\r\n      this.$nextTick(function() {\r\n        self.setAllamount(this.ItemShow);\r\n      });\r\n    },\r\n    setAllamount() {\r\n      this.allamount = 0;\r\n      for (let i = 0; i < this.ItemShow.length; i++) {\r\n        this.allamount += this.ItemShow[i].allamount;\r\n      }\r\n    },\r\n\r\n    checkStatusWi() {\r\n      let config = {\r\n        headers: {\r\n          Authorization: `Bearer ${this.$cookies.get(\"token_main\")}`,\r\n        },\r\n      };\r\n\r\n      this.axios\r\n        .get(this.$API_URL + \"/api/v1/frontend/checkedwiwalletverify\", config)\r\n        .then((response) => {\r\n          if (response.data.status === \"pending\") {\r\n            this.wistatus = \"pending\";\r\n          } else if (response.data.status === \"checking\") {\r\n            this.wistatus = \"checking\";\r\n          } else if (response.data.status === \"success\") {\r\n            this.wistatus = \"success\";\r\n          } else if (response.data.status === \"fail\") {\r\n            this.wistatus = \"fail\";\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          this.errorMessage = error.message;\r\n          this.wistatus = 0;\r\n        });\r\n    },\r\n\r\n    getVerifyStatus() {\r\n      let self = this;\r\n\r\n      this.axios\r\n        .get(this.$API_URL + \"/api/v1/frontend/getuserbalance\", {\r\n          headers: {\r\n            Authorization: `Bearer ${this.$cookies.get(\"token_main\")}`,\r\n          },\r\n        })\r\n        .then((response) => {\r\n          // console.log(response.data.data);\r\n\r\n          this.isVerifyWiwallet = response.data.data.is_verify_wiwallet;\r\n          this.isVerifyOTP = response.data.data.is_verify_otp;\r\n          this.$nextTick(function() {\r\n            // console.log(\"iswiwallet--\" + this.isVerifyWiwallet);\r\n            // console.log(\"isotp--\" + this.isVerifyOTP);\r\n            // console.log(\"Wistatus--\" + this.Wistatus);\r\n            if (self.isVerifyOTP == true && self.isVerifyWiwallet == true) {\r\n              self.TradeStetus = true;\r\n            }\r\n          });\r\n        });\r\n    },\r\n    trade() {\r\n      // const self = this;\r\n\r\n      console.log(\"trade success\");\r\n      console.log(this.SelectItem);\r\n      this.$cookies.set(\"SelectNum\", \"\");\r\n\r\n      var memberStoreId = [];\r\n\r\n      this.SelectItem.length;\r\n      for (let i = 0; i < this.SelectItem.length; i++) {\r\n        memberStoreId.push(this.SelectItem[i].track_id);\r\n      }\r\n      console.log(memberStoreId);\r\n      var body = new Object();\r\n\r\n      body = {\r\n        member_store_id: memberStoreId,\r\n\r\n      };\r\n\r\n      let config = {\r\n        headers: { Authorization: `Bearer ${this.$cookies.get(\"token_main\")}` },\r\n      };\r\n      this.$confirm(\"ยืนยันการแลกเปลี่ยน\").then(() => {\r\n        this.axios\r\n\r\n          .post(this.$API_URL + \"/api/v1/frontend/member/withdraw-item\", body, config)\r\n          .then((response) => {\r\n            if (response.data.code === 0) {\r\n              console.log(response.data.code);\r\n              document.getElementById(\"dismiss\").click();\r\n              this.$alert(\"\", \"แลกเปลี่ยนสำเร็จ\", \"success\").then(() => {\r\n                location.replace(\"/Myitems\");\r\n              });\r\n              setTimeout(() => {\r\n                location.replace(\"/Myitems\");\r\n              }, 3000);\r\n              // self.$router.push({\r\n              //   name: \"Myitems\",\r\n              // });\r\n            } else {\r\n              console.log(response.data.code);\r\n              this.$alert(\r\n                \"ไม่สามารถแลกเปลี่ยนไอเท็มได้ กรุณาติดต่อแอดมิน\",\r\n                \"เกิดข้อผิดพลาด\",\r\n                \"error\"\r\n              );\r\n            }\r\n          })\r\n          .catch((error) => {\r\n            this.errorMessage = error.message;\r\n            console.log(error.response);\r\n            alert(error);\r\n          });\r\n      });\r\n    },\r\n  },\r\n\r\n  computed: {},\r\n\r\n  watch: {\r\n    SelectItem() {\r\n      let group = [];\r\n      group = this.SelectItem.reduce((group, item) => {\r\n        const { item_id } = item;\r\n        group[item_id] = group[item_id] ?? [];\r\n        group[item_id].push(item);\r\n        return group;\r\n      }, {});\r\n\r\n      this.setShowItem(group);\r\n    },\r\n    // VerifyStatus() {\r\n    //   console.log(this.VerifyStatus);\r\n    // },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components/modal"}]}