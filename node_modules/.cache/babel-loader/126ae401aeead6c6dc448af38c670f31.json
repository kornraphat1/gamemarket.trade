AA,IAAA;;AAEA,UAAA,KAAA,WAAA,EAAA;AACA,aAAA,OAAA;AACA,eAAA,KAAA,OAAA;AACA,OAHA,MAGA;AACA,aAAA,OAAA;AACA,QAAA,IAAA,CAAA,WAAA,GAAA,EAAA;AACA,eAAA,KAAA,OAAA;AACA;AACA,KAZA;AAaA,IAAA,QAbA,sBAaA;AACA;AACA,UAAA,IAAA,GAAA,IAAA;;AACA,UAAA,KAAA,QAAA,IAAA,EAAA,EAAA;AACA,aAAA,OAAA;AACA,eAAA,KAAA,OAAA;AACA,OAHA,MAGA;AACA,aAAA,OAAA;AACA,QAAA,IAAA,CAAA,QAAA,GAAA,EAAA;AACA,eAAA,KAAA,OAAA;AACA;AACA,KAxBA;AA0BA,IAAA,SA1BA,uBA0BA;AACA,UAAA,IAAA,GAAA,IAAA;;AACA,UAAA,KAAA,SAAA,IAAA,EAAA,EAAA;AACA,aAAA,OAAA;AACA,eAAA,KAAA,OAAA;AACA,OAHA,MAGA;AACA,aAAA,OAAA;AACA,QAAA,IAAA,CAAA,SAAA,GAAA,EAAA;AACA,eAAA,KAAA,OAAA;AACA;AACA;AApCA;AArIA,CAAA","sourcesContent":["<template>\n  <div>\n    <div class=\"bg-tradeDetail\">\n      <div class=\"content-marketplaceDetail  \">\n        <div style=\"width: 100%;\">\n          <img src=\"/img/Rectangle 270.png\" alt=\"\" style=\"width: 100%;\" />\n        </div>\n      </div>\n    </div>\n\n    <div class=\"bg-third\" id=\"section-third\">\n      <div class=\"container-content\">\n        <div class=\"marketplace-Search-area row\">\n          <div class=\" col-xs-12 col-md-12 col-lg-7\">\n            <div class=\"input-group-prepend\">\n              <span class=\"\" id=\"basic-addon1\"\n                ><i class=\"fas fa-search\"></i\n              ></span>\n              <input\n                type=\"text\"\n                v-model=\"searchQuery\"\n                placeholder=\"Search Product\"\n              />\n            </div>\n          </div>\n          <div class=\"row col-xs-12 col-md-12 col-lg-5 sm-market-sort\">\n            <div class=\"col-6\">\n              <select\n                class=\" marketplace-fillter-title  dropdown-toggle\"\n                aria-label=\"Default select example\"\n                v-model=\"sortGame\"\n              >\n                <option class=\"dropd-item\" selected value=\"\">All Game</option>\n                <option\n                  v-for=\"(v, key) in Allgame\"\n                  :key=\"key\"\n                  class=\"dropd-item\"\n                  :value=\"v.id\"\n                  >{{ v.short_name }}</option\n                >\n              </select>\n            </div>\n            <div class=\"col-6\">\n              <select\n                class=\" marketplace-fillter-title  dropdown-toggle\"\n                aria-label=\"Default select example\"\n                v-model=\"sortPrice\"\n              >\n                <option class=\"dropd-item\" selected value=\"\">Sort by</option>\n                <option class=\"dropd-item\" value=\"asc\"\n                  >Price: Low to high</option\n                >\n                <option class=\"dropd-item\" value=\"desc\"\n                  >Price: higt to low</option\n                >\n              </select>\n            </div>\n          </div>\n        </div>\n        <div class=\"content-third-area row\">\n          <div class=\"content-third-item \" id=\"allitem\">\n            <div class=\"row\" v-if=\"Allitem.length > 0\">\n              <div\n                class=\"item-grid col-3\"\n                v-for=\"(v, key) in Allitem\"\n                :key=\"key\"\n              >\n                <router-link\n                  class=\"static\"\n                  :to=\"{\n                    path: '/trade/' + v.game_id + '/' + v.id,\n                  }\"\n                >\n                  <div class=\"item-img\">\n                    <img\n                      :src=\"v.picture ? $IMG_URL_ITEM + v.picture : $IMG_image\"\n                    />\n                  </div>\n                  <div class=\"item-detail\">\n                    <h2 :style=\"v.color_code | color_sty\">\n                      {{ v.name.en }}\n                    </h2>\n                    <h1>{{ v.name.th }}</h1>\n                  </div>\n                  <div class=\"row item-detail-price\">\n                    <p>{{ v.amount | number_format }}.-</p>\n                    <!-- <button class=\"btn\">Trade</button> -->\n                  </div>\n                </router-link>\n              </div>\n              <div class=\"Pagination\" v-if=\"max_page>1\">\n                <div class=\"Pagination-item\">\n                  <button\n                    class=\"btn\"\n                    @click=\"PaginationFirst()\"\n                    id=\"PaginationFirst\"\n                    :disabled=\"pagi == 1 ? '' : disabled\"\n                  >\n                    <i class=\"fas fa-angle-double-left\"></i>\n                  </button>\n                </div>\n                <div class=\"Pagination-item\">\n                  <button\n                    class=\"btn\"\n                    @click=\"PaginationBack()\"\n                    id=\"PaginationBack\"\n                    :disabled=\"pagi == 1 ? '' : disabled\"\n                  >\n                    <i class=\"fas fa-angle-left\"></i>\n                  </button>\n                </div>\n\n                <div\n                  v-for=\"item in max_page \"\n                  :key=\"item\"\n                  :class=\"Paginationitem(item)\"\n                >\n                  <button class=\"btn\" @click=\"Pagination(item)\">\n                    {{ item }}\n                  </button>\n                </div>\n                <div class=\"Pagination-item\">\n                  <button\n                    class=\"btn\"\n                    @click=\"PaginationNext()\"\n                    id=\"PaginationNext\"\n                    :disabled=\"pagi == max_page ? '' : disabled\"\n                  >\n                    <i class=\"fas fa-angle-right\"></i>\n                  </button>\n                </div>\n                <div class=\"Pagination-item\">\n                  <button\n                    class=\"btn\"\n                    @click=\"PaginationEnd()\"\n                    id=\"PaginationEnd\"\n                    :disabled=\"pagi == max_page ? '' : disabled\"\n                  >\n                    <i class=\"fas fa-angle-double-right\"></i>\n                  </button>\n                </div>\n              </div>\n            </div>\n            <div v-else>\n              <div style=\"color: #fff;\" class=\"text-center\">\n                <h1>ไม่พบไอเท็มที่ ค้นหา</h1>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      Allitem: [],\n      Allgame: [],\n\n      max_page: \"\",\n      searchQuery: \"\",\n      sortGame: \"\",\n      sortPrice: \"\",\n      token: \"\",\n      pagi: 1,\n      pagishow: [],\n    };\n  },\n\n  mounted() {\n    // const tis = this;\n    this.changeSelected(1);\n\n    this.checkToken();\n    this.getAllgame();\n    this.getData();\n   \n  },\n  computed: {},\n  methods: {\n    getAllgame() {\n      this.axios\n        .get(this.$API_URL + \"/api/v1/frontend/getlistgame\")\n        .then((response) => {\n          this.Allgame = response.data.data;\n          // console.log(this.Allgame);\n        });\n    },\n    getData() {\n      const self = this;\n      var body = new Object();\n      if (this.sortGame != \"\") {\n        body = {\n          filter: {\n            game_id: self.sortGame,\n            search: self.searchQuery,\n          },\n          perpage: 16,\n          page: this.pagi,\n          pagination_on: false,\n          sort_by: self.sortPrice,\n        };\n      } else {\n        body = {\n          filter: {\n            search: self.searchQuery,\n          },\n          perpage: 16,\n          page: this.pagi,\n          pagination_on: false,\n          sort_by: self.sortPrice,\n        };\n      }\n      const config = {\n        headers: { Authorization: `Bearer ${this.$cookies.get(\"token_main\")}` },\n      };\n      // console.log(this.$cookies.get(\"token\"));\n      this.axios\n        .post(\n          this.$API_URL + \"/api/v1/frontend/marketplace/listitem\",\n          body,\n          config\n        )\n        .then((response) => {\n          // console.log(response.data.data.list);\n          this.Allitem = response.data.data.list;\n          this.max_page = response.data.data.max_page;\n          this.$nextTick(function() {\n            self.SetPagination();\n          });\n        })\n        .catch((error) => {\n          this.errorMessage = error.message;\n          console.log(error);\n          // alert(error);\n        });\n    },\n  SetPagination() {\n\n    \n     let is = 0;\n    while (is < this.max_page) {\n      console.log(is);\n      is++;\n    }\n\n\n    },\n    Pagination(item) {\n      this.pagi = item;\n      this.getData();\n      document.getElementById(\"section-third\").scrollIntoView();\n    },\n    PaginationBack() {\n      this.pagi = this.pagi - 1;\n\n      this.getData();\n      document.getElementById(\"section-third\").scrollIntoView();\n    },\n    PaginationNext() {\n      this.pagi = this.pagi + 1;\n\n      this.getData();\n      document.getElementById(\"section-third\").scrollIntoView();\n    },\n    PaginationFirst() {\n      this.pagi = this.pagi = 1;\n\n      this.getData();\n      document.getElementById(\"section-third\").scrollIntoView();\n    },\n    PaginationEnd() {\n      this.pagi = this.pagi = this.max_page;\n\n      this.getData();\n      document.getElementById(\"section-third\").scrollIntoView();\n    },\n\n    Paginationitem(item) {\n      if (this.pagi == item) {\n        return \"Pagination-item active\";\n      } else {\n        return \"Pagination-item\";\n      }\n    },\n  },\n  watch: {\n    searchQuery() {\n      const self = this;\n\n      if (this.searchQuery) {\n        this.getData();\n        return this.Allitem;\n      } else {\n        this.getData();\n        self.searchQuery = \"\";\n        return this.Allitem;\n      }\n    },\n    sortGame() {\n      // console.log(this.sortGame);\n      const self = this;\n      if (this.sortGame != \"\") {\n        this.getData();\n        return this.Allitem;\n      } else {\n        this.getData();\n        self.sortGame = \"\";\n        return this.Allitem;\n      }\n    },\n\n    sortPrice() {\n      const self = this;\n      if (this.sortPrice != \"\") {\n        this.getData();\n        return this.Allitem;\n      } else {\n        this.getData();\n        self.sortPrice = \"\";\n        return this.Allitem;\n      }\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}