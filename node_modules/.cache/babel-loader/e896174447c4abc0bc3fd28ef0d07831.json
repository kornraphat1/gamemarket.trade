unted() {\r\n    this.getData();\r\n  },\r\n  components: {\r\n    Flickity,\r\n    Swiper,\r\n    SwiperSlide,\r\n  },\r\n  setup() {\r\n    const onSwiper = (swiper) => {\r\n      console.log(swiper);\r\n    };\r\n    const onSlideChange = () => {\r\n      console.log(\"slide change\");\r\n    };\r\n    return {\r\n      onSwiper,\r\n      onSlideChange,\r\n    };\r\n  },\r\n  methods: {\r\n    getData() {\r\n      // const self = this;\r\n      this.axios\r\n        .get(this.$API_URL + \"/api/v1/user/getlistgame\")\r\n        .then((response) => {\r\n          // console.log(response.data.data)\r\n\r\n          this.Allgame = response.data.data;\r\n\r\n          // this.$nextTick(function() {\r\n          //   self.flktygame(); // DOM updated\r\n          // });\r\n        });\r\n    },\r\n\r\n    next() {\r\n      this.$refs.flickity.next();\r\n    },\r\n\r\n    previous() {\r\n      // console.log(\"previous\");\r\n      this.$refs.flickity.previous();\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components/home"}]}