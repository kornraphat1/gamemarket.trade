ow\">\n              <!--  <div\n                id=\"title-tel\"\n                class=\"col-4 vertify-item \"\n                style=\"opacity: 1;\"\n              >\n                <div class=\"number-nav\">\n                  <span>1</span>\n                </div>\n\n                <p>เบอร์</p>\n                <p>โทรศัพท์</p>\n              </div>-->\n              <div\n                id=\"title-otp\"\n                class=\"col-6 vertify-item \"\n                style=\"opacity: 1;\"\n              >\n                <div class=\"number-nav\">\n                  <span>1</span>\n                </div>\n\n                <p>ยืนยัน</p>\n                <p>OTP</p>\n              </div>\n              <div\n                id=\"title-pass\"\n                class=\"col-6 vertify-item \"\n                style=\"opacity: .5;\"\n              >\n                <div class=\"number-nav\">\n                  <span>2</span>\n                </div>\n\n                <p>สร้าง</p>\n                <p>รหัสผ่าน</p>\n              </div>\n            </div>\n            <!-- <input\n                id=\"tel\"\n                type=\"text\"\n                placeholder=\"กรุณาใส่เบอร์โทรศัพท์\"\n                v-model=\"tel\"\n                style=\"display:block\"\n              />\n            -->\n            <div class=\"vertify-input\" id=\"otp\">\n              <div class=\"otp-zone\">\n                <v-otp-input\n                  ref=\"otpInput\"\n                  input-classes=\"otp-input\"\n                  separator=\"\"\n                  :num-inputs=\"6\"\n                  :should-auto-focus=\"true\"\n                  :is-input-num=\"true\"\n                  @on-complete=\"handleOnComplete\"\n                  v-model=\"otp\"\n                />\n\n                <p>\n                  Didn't receive the SMS?\n                  <span @click=\"Sendagain()\"> Send again</span>\n                </p>\n                <span>Reference : {{ refno }}</span>\n                <!-- <a href=\"\">Enter different phone number</a> -->\n              </div>\n              <div class=\"vertify-button\">\n                <button v-on:click=\"otpnext()\">ถัดไป</button>\n              </div>\n            </div>\n\n            <div class=\"vertify-input\" id=\"pass\" style=\"display:\">\n              <div>\n                <vuePassword\n                  :type=\"passwordFieldType\"\n                  placeholder=\"สร้างรหัสผ่าน\"\n                  :disableStrength=\"true\"\n                  v-model=\"pass\"\n                />\n\n                <vuePassword\n                  :type=\"passwordFieldType\"\n                  placeholder=\"ยืนยันรหัสผ่าน\"\n                  :disableStrength=\"true\"\n                  v-model=\"passcon\"\n                />\n              </div>\n              <div class=\"alert_pass\">\n                <p>\n                  รหัสผ่านจะต้องประกอบไปด้วยตัวอักษร\n                </p>\n                <p>พิมพ์เล็ก,พิมพ์ใหญ่,และอักขระพิเศษ</p>\n              </div>\n              <div class=\"vertify-button\">\n                <button v-on:click=\"PassWnext()\">ถัดไป</button>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      tel: \"\",\n      otp: \"\",\n      pass: \"\",\n      passcon: \"\",\n      verify_link: \"\",\n      token: \"\",\n      token_otp: \"\",\n      refno: \"\",\n      phone: \"\",\n      passwordFieldType: \"password\",\n    };\n  },\n\n  mounted() {\n    document.getElementById(\"footer-contener\").style.display = \"none\";\n    document.getElementById(\"navbar\").style.display = \"none\";\n    this.verify_link = this.$route.query.verify_link;\n    this.axios\n      .get(\n        this.$API_URL +\n          \"/api/v1/otp/get-response/\" +\n          this.$route.query.verify_link\n      )\n      .then((response) => {\n        this.token_otp = response.data.result.token;\n        this.phone = response.data.result.phone;\n        this.refno = response.data.result.refno;\n      })\n      .catch((error) => {\n        // const self = this;\n        this.errorMessage = error.message;\n        console.log(error.response.data.meg.th);\n        if (error.response.data.code === 1421) {\n          // self.Sendagain();\n        }\n      });\n  },\n\n  methods: {\n    Sendagain() {\n      // console.log(this.token_otp);\n      const self = this;\n      let Sendagain_con = {\n        headers: { Authorization: `Bearer ${this.token}` },\n      };\n      let Sendagain_body = { token: this.token_otp, phone: this.phone };\n\n      this.axios\n        .post(\n          this.$API_URL + \"/api/v1/otp/resend\",\n          Sendagain_body,\n          Sendagain_con\n        )\n        .then((response) => {\n          console.log(response.data.res.unique_id);\n          self.$router.push({\n            name: \"Vertify\",\n            query: { verify_link: response.data.res.unique_id },\n          });\n        })\n        .catch((error) => {\n          this.errorMessage = error.message;\n          console.log(error.response);\n          alert(error.response.data.msg.th);\n        });\n    },\n    otpnext() {\n      const self = this;\n      if (this.otp) {\n        if (this.token_otp) {\n          let body = { token: this.token_otp, pin: this.otp };\n          let config = {\n            headers: { Authorization: `Bearer ${this.token}` },\n          };\n\n          this.axios\n            .post(this.$API_URL + \"/api/v1/otp/send-verify\", body, config)\n            .then((response) => {\n              if (response.data.msg == \"success\") {\n                if (response.data.result.register_success == true) {\n                  self.$router.push({\n                    name: \"Login\",\n                  });\n                } else {\n                  console.log(response.data.msg);\n                  document.getElementById(\"otp\").style.display = \"none\";\n                  document.getElementById(\"pass\").style.display = \"flex\";\n                  document.getElementById(\"title-otp\").style.opacity = \".5\";\n                  document.getElementById(\"title-pass\").style.opacity = \"1\";\n                }\n              }\n            })\n            .catch((error) => {\n              this.errorMessage = error.message;\n              console.log(error.response);\n              alert(error.response.data.msg.th);\n              self.handleClearInput();\n            });\n        }\n      }\n    },\n\n    PassWnext() {\n      const self = this;\n      if (this.pass && this.passcon) {\n        if (this.pass === this.passcon) {\n          let body = {\n            token: this.token_otp,\n            password: this.pass,\n            password_confirm: this.passcon,\n          };\n          let config = {\n            headers: {},\n          };\n\n          this.axios\n            .post(\n              this.$API_URL + \"/api/v1/otp/game-member-new-regis\",\n              body,\n              config\n            )\n            .then((response) => {\n              if (response.data.code == 0) {\n                console.log(response.data);\n                self.$router.push({\n                  name: \"Login\",\n                });\n              } else {\n                alert(response.data.msg.th);\n              }\n            })\n            .catch((error) => {\n              this.errorMessage = error.message;\n              console.log(error.response);\n              alert(error.response);\n              self.handleClearInput();\n            });\n        } else {\n          alert(\"รหัสผ่าน ไม่ตรงกัน\");\n        }\n      }\n    },\n\n    handleOnComplete(value) {\n      this.otp = value;\n    },\n    handleClearInput() {\n      this.$refs.otpInput.clearInput();\n    },\n    switchVisibility() {\n      this.passwordFieldType =\n        this.passwordFieldType === \"password\" ? \"text\" : \"password\";\n    },\n  },\n  watch: {},\n  computed: {},\n};\n</script>\n"],"sourceRoot":"src/views"}]}