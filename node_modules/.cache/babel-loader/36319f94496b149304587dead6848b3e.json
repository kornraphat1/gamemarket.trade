r\nimport Flickity from \"vue-flickity\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      Allgame: [],\r\n      flickityready: \"\",\r\n      flickityOptions: {\r\n        lazyLoad: true,\r\n        cellAlign: \"left\",\r\n        contain: true,\r\n        // groupCells: \"50%\",\r\n        // freeScroll: true,\r\n        wrapAround: true,\r\n        prevNextButtons: false,\r\n        pageDots: false,\r\n        on: {\r\n          dragStart: () => {\r\n            document.querySelector(\".main-caro\").style.pointerEvents = \"none\";\r\n          },\r\n          dragEnd: () => {\r\n            document.querySelector(\".main-caro\").style.pointerEvents = \"all\";\r\n          },\r\n          ready: () => {\r\n            // this.flickityready = \"ready\";\r\n            console.log( \"ready\");\r\n          },\r\n        },\r\n        // any options from Flickity can be used\r\n      },\r\n    };\r\n  },\r\n  mounted() {\r\n    // const self = this;\r\n    this.getData();\r\n    \r\n  \r\n\r\n    // self.$refs.flickity.rerender()\r\n  },\r\n  components: {\r\n    Flickity,\r\n  },\r\n  methods: {\r\n \r\n    getData() {\r\n      this.axios\r\n        .get(this.$API_URL + \"/api/v1/frontend/getlistgame\")\r\n        .then((response) => {\r\n          this.Allgame = response.data.data;\r\n          this.$nextTick(function() {\r\n            var element = document.getElementById(\"game1\");\r\n            element.classList.add(\"game-select\");\r\n          });\r\n        });\r\n    },\r\n    sentIdgame(id) {\r\n      console.log(id);\r\n      this.$emit(\"idgame\", id);\r\n    },\r\n\r\n    next() {\r\n      console.log(this.$refs.flickity);\r\n      this.$refs.flickity.next();\r\n    },\r\n\r\n    previous() {\r\n      // this.$refs.flickity.rerender()\r\n      console.log(\"previous\");\r\n      this.$refs.flickity.previous();\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components/home"}]}