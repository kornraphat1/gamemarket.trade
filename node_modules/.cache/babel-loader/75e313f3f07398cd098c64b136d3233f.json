CAAA,OAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA;AACA,YAAA,KAAA,CAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA;AACA,YAAA,IAAA,CAAA,gBAAA;AACA,WAhBA;AAiBA;AACA;;AACA,UAAA,KAAA,IAAA,EAAA;AACA,QAAA,QAAA,CAAA,cAAA,CAAA,uBAAA,EAAA,KAAA,CAAA,OAAA,GAAA,MAAA;AACA,QAAA,QAAA,CAAA,cAAA,CAAA,yBAAA,EAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AACA;AACA,KAxEA;AA0EA,IAAA,gBA1EA,4BA0EA,KA1EA,EA0EA;AACA,WAAA,GAAA,GAAA,KAAA;AACA,KA5EA;AA6EA,IAAA,gBA7EA,8BA6EA;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,UAAA;AACA,KA/EA;AAgFA,IAAA,gBAhFA,8BAgFA;AACA,WAAA,iBAAA,GACA,KAAA,iBAAA,KAAA,UAAA,GAAA,MAAA,GAAA,UADA;AAEA;AAnFA,GA3CA;AAgIA,EAAA,KAAA,EAAA,EAhIA;AAiIA,EAAA,QAAA,EAAA;AAjIA,CAAA","sourcesContent":["<template>\n  <div>\n    <div class=\"bg-vertify\">\n      <div class=\"bg-b-vertify\"></div>\n    </div>\n\n    <div class=\"vertify-main-content\">\n      <div class=\"logo-vertify\">\n        <img src=\"/img/logo-vertify.png\" alt=\"\" />\n      </div>\n      <div class=\"main-vertify-bg\" style=\"  height:80vh\">\n        <div class=\"main-vertify\">\n          <div class=\"vertify-content\" id=\"vertify-content-input\">\n            <div class=\"vertify-content-header\">\n              <span>ยืนยันตัวตน</span>\n            </div>\n            <div class=\"vertify-content-nav row\">\n              <!--  <div\n                id=\"title-tel\"\n                class=\"col-4 vertify-content-item \"\n                style=\"opacity: 1;\"\n              >\n                <div class=\"number-nav\">\n                  <span>1</span>\n                </div>\n\n                <p>เบอร์</p>\n                <p>โทรศัพท์</p>\n              </div>-->\n              <div\n                id=\"title-otp\"\n                class=\"col-6 vertify-content-item \"\n                style=\"opacity: 1;\"\n              >\n                <div class=\"number-nav\">\n                  <span>1</span>\n                </div>\n\n                <p>ยืนยัน</p>\n                <p>OTP</p>\n              </div>\n              <div\n                id=\"title-pass\"\n                class=\"col-6 vertify-content-item \"\n                style=\"opacity: .5;\"\n              >\n                <div class=\"number-nav\">\n                  <span>2</span>\n                </div>\n\n                <p>สร้าง</p>\n                <p>รหัสผ่าน</p>\n              </div>\n            </div>\n            <!-- <input\n                id=\"tel\"\n                type=\"text\"\n                placeholder=\"กรุณาใส่เบอร์โทรศัพท์\"\n                v-model=\"tel\"\n                style=\"display:block\"\n              />\n            -->\n            <div class=\"vertify-content-input\" id=\"otp\">\n              <div class=\"otp-zone\">\n                <v-otp-input\n                  ref=\"otpInput\"\n                  input-classes=\"otp-input\"\n                  separator=\"\"\n                  :num-inputs=\"6\"\n                  :should-auto-focus=\"true\"\n                  :is-input-num=\"true\"\n                  @on-complete=\"handleOnComplete\"\n                  v-model=\"otp\"\n                />\n\n                <p>\n                  Didn't receive the SMS?\n                  <a @click=\"Sendagain()\"> Send again</a>\n                </p>\n                <span>Reference : {{ refno }}</span>\n                <!-- <a href=\"\">Enter different phone number</a> -->\n              </div>\n              <div class=\"vertify-content-button\">\n                <button v-on:click=\"otpnext()\">ถัดไป</button>\n              </div>\n            </div>\n\n            <div class=\"vertify-content-input\" id=\"pass\" style=\"display:none\">\n              <div>\n                <vuePassword\n                  :type=\"passwordFieldType\"\n                  placeholder=\"สร้างรหัสผ่าน\"\n                  :disableStrength=\"true\"\n                  v-model=\"pass\"\n                />\n\n                <vuePassword\n                  :type=\"passwordFieldType\"\n                  placeholder=\"ยืนยันรหัสผ่าน\"\n                  :disableStrength=\"true\"\n                  v-model=\"passcon\"\n                />\n              </div>\n              <div class=\"vertify-content-button\">\n                <button v-on:click=\"clicknext()\">ถัดไป</button>\n              </div>\n            </div>\n          </div>\n\n          <div\n            class=\"vertify-content\"\n            id=\"vertify-content-success\"\n            style=\"display:none\"\n          >\n            <div class=\"vertify-content-success\">\n              <img src=\"/img/success.png\" alt=\"\" />\n              <h4>ยืนยันตัวตนเรียบร้อย</h4>\n              <span>เริ่มซื้อขายไอเท็มได้เลย !</span>\n              <button v-on:click=\"clicknext()\">แลกเปลี่ยนไอเท็ม</button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      tel: \"\",\n      otp: \"\",\n      pass: \"\",\n      passcon: \"\",\n      verify_link: \"\",\n      token: \"\",\n      token_otp: \"\",\n      refno: \"\",\n      phone: \"\",\n      passwordFieldType: \"password\",\n    };\n  },\n\n  mounted() {\n      this.gettoken();\n    document.getElementById(\"footer-contener\").style.display = \"none\";\n    document.getElementById(\"navbar\").style.display = \"none\";\n    this.verify_link = this.$route.query.verify_link;\n    this.axios\n      .get(\n        this.$API_URL +\n          \"/api/v1/otp/get-response/\" +\n          this.$route.query.verify_link\n      )\n      .then((response) => {\n\n        this.token_otp = response.data.result.token;\n        this.phone = response.data.result.phone;\n        this.refno = response.data.result.refno;\n\n       \n      })\n      .catch((error) => {\n        this.errorMessage = error.message;\n        console.log(error.response.data.msg.th);\n\n      \n      });\n  },\n\n  methods: {\n    gettoken() {\n      this.axios.get(this.$API_URL + \"/api/v1/token/new\").then((response) => {\n        // console.log(response.data.access_token);\n        let D = new Date();\n        D.setHours(D.getHours() + 1);\n\n        console.log(D);\n\n        //  this.$cookie.set(\"token\", response.data.access_token, D)\n\n        document.cookie = \"token=\" + response.data.access_token + \";\" + D;\n        this.token = this.$cookies.get(\"token\");\n      });\n    },\n    Sendagain() {\n        // console.log(this.token_otp);\n\n      let Sendagain_con = {\n        headers: { Authorization: `Bearer ${this.token}` },\n      };\n      let Sendagain_body = { token: this.token_otp, phone: this.phone };\n\n      this.axios\n        .post(\n          this.$API_URL + \"/api/v1/otp/resend\",\n          Sendagain_body,\n          Sendagain_con\n        )\n        .then((response) => {\n          console.log(response.res);\n       \n        })\n        .catch((error) => {\n          this.errorMessage = error.message;\n          console.log(error.response);\n          alert(error.response.data.msg.th);\n          \n        });\n    },\n    otpnext() {\n      const self = this;\n      if (this.otp) {\n        if (this.token_otp) {\n          let body = { token: this.token_otp, pin: this.otp };\n          let config = {\n            headers: { Authorization: `Bearer ${this.token}` },\n          };\n\n          this.axios\n            .post(this.$API_URL + \"/api/v1/otp/send-verify\", body, config)\n            .then((response) => {\n              if (response.data.msg == \"success\") {\n                console.log(response.data.msg);\n                document.getElementById(\"otp\").style.display = \"none\";\n                document.getElementById(\"pass\").style.display = \"flex\";\n                document.getElementById(\"title-otp\").style.opacity = \".5\";\n                document.getElementById(\"title-pass\").style.opacity = \"1\";\n              }\n            })\n            .catch((error) => {\n              this.errorMessage = error.message;\n              console.log(error.response);\n              alert(error.response.data.msg.th);\n              self.handleClearInput();\n            });\n        }\n      }\n      if (this.pass) {\n        document.getElementById(\"vertify-content-input\").style.display = \"none\";\n        document.getElementById(\"vertify-content-success\").style.display = null;\n      }\n    },\n\n    handleOnComplete(value) {\n      this.otp = value;\n    },\n    handleClearInput() {\n      this.$refs.otpInput.clearInput();\n    },\n    switchVisibility() {\n      this.passwordFieldType =\n        this.passwordFieldType === \"password\" ? \"text\" : \"password\";\n    },\n  },\n  watch: {},\n  computed: {},\n};\n</script>\n"],"sourceRoot":"src/views"}]}