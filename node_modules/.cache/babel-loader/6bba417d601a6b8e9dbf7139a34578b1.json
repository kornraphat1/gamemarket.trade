kground-content.png\"\r\n            alt=\"\"\r\n            class=\"content-first-bg\"\r\n          />\r\n          <div id=\"content-first\" class=\"content-first-area\">\r\n            <img\r\n              src=\"img/logo-name-market.png\"\r\n              alt=\"\"\r\n              class=\"content-first-img\"\r\n            />\r\n\r\n            <router-link\r\n              class=\"content-first-btn btn\"\r\n              :to=\"{\r\n                name: 'Myitems',\r\n              }\"\r\n            >\r\n              TRADE NOW\r\n            </router-link>\r\n          </div>\r\n          <img src=\"img/Rectangle.png\" alt=\"\" class=\"content-bg-connect\" />\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"bg-second\">\r\n      <div class=\"container-content\">\r\n        <div class=\"content-second\">\r\n          <div class=\"content-second-area row\">\r\n            <div class=\"content-second-aboutme col-6\">\r\n              <h2>About</h2>\r\n              <h1>Game Market Shop</h1>\r\n              <p>\r\n                Lorem Ipsum is simply dummy text of the printing and typesetting\r\n                industry. Lorem Ipsum has been the industry's standard dummy\r\n                text ever since the 1500s, when an Lorem Ipsum PageMaker\r\n                including versions of Lorem Ipsum.\r\n              </p>\r\n              <!-- <button class=\"content-btn-readmore btn\">\r\n                Read more About us\r\n              </button>-->\r\n            </div>\r\n\r\n            <div class=\"content-second-logo col-6\">\r\n              <img\r\n                src=\"/img/content-logo.png\"\r\n                class=\"content-second-logo-img\"\r\n              />\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"empty-space\"></div>\r\n      </div>\r\n    </div>\r\n    <div class=\"bg-third\" id=\"section-third\">\r\n      <!-- <Allgame @idgame=\"idgame\"></Allgame> -->\r\n<div id=\"section-third\">\r\n      <div class=\"empty-space\"></div>\r\n      <div class=\"container-content \">\r\n        <div class=\"content-third-itile row\">\r\n          <div class=\"col-6\">\r\n            <h2>เกมส์ทั้งหมด</h2>\r\n          </div>\r\n          <div class=\"col-6 previous-next-zone\">\r\n            <button class=\"previous-next-btn \" @click=\"previous()\">\r\n              <i class=\"fas fa-angle-left\"></i>\r\n            </button>\r\n\r\n            <button class=\"previous-next-btn \" @click=\"next()\">\r\n              <i class=\"fas fa-angle-right\"></i>\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"content-game-area container-allgame \">\r\n        <flickity\r\n          class=\"main-caro \"\r\n          ref=\"flickity\"\r\n          :options=\"flickityOptions\"\r\n          v-if=\"Allgame.length > 0\"\r\n        >\r\n          <div\r\n            class=\"carousel-cell-game \"\r\n            :id=\"'game' + v.id\"\r\n            v-for=\"(v, key) in Allgame\"\r\n            :key=\"key\"\r\n            @click=\"sentIdgame(v.id)\"\r\n          >\r\n            <div class=\"img-game\">\r\n              <img :src=\"$IMG_URL + v.logo\" />\r\n            </div>\r\n          </div>\r\n        </flickity>\r\n      </div>\r\n    </div>\r\n      <div class=\"container-content\">\r\n        <div class=\"empty-space\"></div>\r\n        <div class=\"content-third-area row\">\r\n          <div class=\"content-third-itile col-12\">\r\n            <h2>ไอเท็มมาแรง 🔥</h2>\r\n          </div>\r\n          <div id=\"HotItem\" style=\"\" class=\"content-third-item row\">\r\n            <div class=\"item-grid col-3\" v-for=\"(v, key) in hotItem\" :key=\"key\">\r\n              <div class=\"item-img\">\r\n                <img :src=\"($IMG_URL_ITEM + v.picture)\" />\r\n              </div>\r\n              <div class=\"item-detail\">\r\n                <h2 :style=\"v.color_code | color_sty\">\r\n                  {{ v.name.en }}\r\n                </h2>\r\n                <h1>{{ v.name.th }}</h1>\r\n              </div>\r\n              <div class=\"row item-detail-price\">\r\n                <p>{{ v.amount | number_format }}.-</p>\r\n                <!-- <button class=\"btn\">Trade</button> -->\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"content-third-area row\" style=\"\">\r\n          <div class=\"content-third-itile col-12\">\r\n            <h2>รับซื้อราคาดี</h2>\r\n          </div>\r\n          <div class=\"content-third-item row\" id=\"itemGoodPrice\">\r\n            <div\r\n              class=\"item-grid col-3\"\r\n              v-for=\"(v, key) in itemGoodPrice\"\r\n              :key=\"key\"\r\n            >\r\n             <div class=\"item-img\">\r\n                <img :src=\"$IMG_URL_ITEM + v.picture\" />\r\n              </div>\r\n              <div class=\"item-detail\">\r\n                <h2 :style=\"v.color_code | color_sty\">\r\n                  {{ v.name.en }}\r\n                </h2>\r\n                <h1>{{ v.name.th }}</h1>\r\n              </div>\r\n              <div class=\"row item-detail-price\">\r\n                <p>{{ v.amount | number_format }}.-</p>\r\n                <!-- <button class=\"btn\">Trade</button> -->\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"content-third-area container-allitem-sm row\">\r\n          <div class=\"content-third-itile col-12\">\r\n            <h2>ไอเท็มทั้งหมด</h2>\r\n          </div>\r\n          <div class=\"content-third-item row\" id=\"Allitemgrd\">\r\n            <div\r\n              class=\"item-grid  col-3\"\r\n              v-for=\"(v, key) in Allitem\"\r\n              :key=\"key\"\r\n            >\r\n              <div class=\"item-img\">\r\n                <img :src=\"$IMG_URL_ITEM + v.picture\" />\r\n              </div>\r\n              <div class=\"item-detail\">\r\n                <h2 :style=\"v.color_code | color_sty\">\r\n                  {{ v.name.en }}\r\n                </h2>\r\n                <h1>{{ v.name.th }}</h1>\r\n              </div>\r\n              <div class=\"row item-detail-price\">\r\n                <p>{{ v.amount | number_format }}.-</p>\r\n                <!-- <button class=\"btn\">Trade</button> -->\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"content-third-area container-allitem\">\r\n      <div class=\"content-third-itile col-12\" style=\"\">\r\n        <h2>ไอเท็มทั้งหมด</h2>\r\n      </div>\r\n      <div class=\"content-third-item\" id=\"Allitemswp\">\r\n        <div class=\"main-carousel\" v-if=\"Allitem.length > 0\">\r\n          <div class=\"carousel-cell\" v-for=\"(v, key) in Allitem\" :key=\"key\">\r\n           <div class=\"item-img item-img-carousel\">\r\n                <img :src=\"$IMG_URL_ITEM + v.picture\" />\r\n              </div>\r\n            <div class=\"item-detail\">\r\n              <h2 :style=\"v.color_code | color_sty\">\r\n                {{ v.name.en }}\r\n              </h2>\r\n\r\n              <h1>{{ v.name.th }}</h1>\r\n            </div>\r\n            <div class=\"row item-detail-price\">\r\n              <p>{{ v.amount | number_format }}.-</p>\r\n              <!-- <router-link\r\n                :to=\"{\r\n                  path: '/trade/' + v.game_id + '/' + v.id,\r\n                }\"\r\n                class=\"btn\"\r\n                >Trade</router-link\r\n              > -->\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Flickity from \"vue-flickity\";\r\n\r\n// import Allgame from \"../components/home/Allgame.vue\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      hotItem: [],\r\n      itemGoodPrice: [],\r\n      Allitem: [], \r\n      Allgame: [],\r\n\r\n      idGame: \"1\",\r\n      flickityOptions: {\r\n        cellAlign: \"left\",\r\n        contain: true,\r\n        groupCells: \"50%\",\r\n        // freeScroll: true,\r\n        wrapAround: true,\r\n        prevNextButtons: false,\r\n        pageDots: false,\r\n        on: {\r\n          dragStart: () => {\r\n            document.querySelector(\".main-caro\").style.pointerEvents = \"none\";\r\n          },\r\n          dragEnd: () => {\r\n            document.querySelector(\".main-caro\").style.pointerEvents = \"all\";\r\n          },\r\n        },\r\n        \r\n      },\r\n    };\r\n  },\r\n\r\n  mounted() {\r\n    this.changeSelected(0);\r\n    // const self = this;\r\n    this.getData();\r\n    this.checkToken();\r\n    this.getGame();\r\n     \r\n  },\r\n  components: {\r\n    Flickity,\r\n  },\r\n  methods: {\r\n    getData() {\r\n      const self = this;\r\n\r\n      this.axios\r\n        .get(this.$API_URL + \"/api/v1/frontend/getlistitem/\" + this.idGame)\r\n        .then((response) => {\r\n          this.hotItem = response.data.datahot;\r\n          this.itemGoodPrice = response.data.databestprice;\r\n          this.Allitem = response.data.dataall;\r\n          this.$nextTick(function() {\r\n            self.flkty(); // DOM updated\r\n          });\r\n        });\r\n    },\r\n     getGame() {\r\n      // const self = this;\r\n      this.axios\r\n        .get(this.$API_URL + \"/api/v1/frontend/getlistgame\")\r\n        .then((response) => {\r\n          this.Allgame = response.data.data;\r\n          this.$nextTick(function() {\r\n            var element = document.getElementById(\"game1\");\r\n            element.classList.add(\"game-select\");\r\n          });\r\n        });\r\n    },\r\n    sentIdgame(val) {\r\n      // const self = this;\r\n\r\n      this.idGame = val;\r\n\r\n      let gamesl = document.getElementsByClassName(\"game-select\");\r\n      gamesl[0].classList.remove(\"game-select\");\r\n\r\n      var element = document.getElementById(\"game\" + val);\r\n      element.classList.add(\"game-select\");\r\n\r\n      this.Allitem = [];\r\n      this.getData();\r\n      // this.$nextTick(function() {\r\n      //   self.flkty(); // DOM updated\r\n      // });\r\n    },\r\n     next() {\r\n      console.log('next');\r\n      this.$refs.flickity.next();\r\n    },\r\n\r\n    previous() {\r\n      console.log(\"previous\");\r\n      this.$refs.flickity.previous();\r\n    },\r\n    flkty() {\r\n      var elem = document.querySelector(\".main-carousel\");\r\n\r\n      new Flickity(elem, {\r\n        cellAlign: \"left\",\r\n        contain: true,\r\n        groupCells: 4,\r\n\r\n        wrapAround: true,\r\n        prevNextButtons: false,\r\n        pageDots: false,\r\n        on: {\r\n          dragStart: () => {\r\n            elem.style.pointerEvents = \"none\";\r\n          },\r\n          dragEnd: () => {\r\n            elem.style.pointerEvents = \"all\";\r\n          },\r\n        },\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views"}]}