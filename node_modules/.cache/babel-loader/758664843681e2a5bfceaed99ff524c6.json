AAEA,QAAA,KAAA,CAAA,SAAA,CAAA,YAAA;AACA,UAAA,IAAA,CAAA,KAAA,GADA,CACA;AACA,SAFA;AAGA,OAhBA;AAiBA,KAvBA;AAwBA,IAAA,KAxBA,mBAwBA;AACA,UAAA,IAAA,GAAA,QAAA,CAAA,aAAA,CAAA,gBAAA,CAAA;AACA,UAAA,QAAA,CAAA,IAAA,EAAA;AACA;AACA;AACA,QAAA,SAAA,EAAA,MAHA;AAIA,QAAA,OAAA,EAAA,IAJA;AAKA,QAAA,UAAA,EAAA,CALA;AAMA,QAAA,UAAA,EAAA,IANA;AAOA,QAAA,eAAA,EAAA,KAPA;AAQA,QAAA,QAAA,EAAA,KARA;AASA;AACA,QAAA,EAAA,EAAA;AACA,UAAA,SAAA,EAAA,qBAAA;AACA,YAAA,IAAA,CAAA,KAAA,CAAA,aAAA,GAAA,MAAA;AACA,WAHA;AAIA,UAAA,OAAA,EAAA,mBAAA;AACA,YAAA,IAAA,CAAA,KAAA,CAAA,aAAA,GAAA,KAAA;AACA;AANA;AAVA,OAAA;AAmBA;AA7CA;AAhBA,CAAA","sourcesContent":["<template>\n  <div>\n    <div class=\"bg-tradeDetail\">\n      <div class=\"container-main content-tradeDetail \">\n        <div class=\"row \">\n          <div class=\"col-6  tradeDetail-left\">\n            <div class=\"img-tradeDetail \">\n              <img :src=\"main_item.picture\" class=\"item-img-detail\" />\n            </div>\n          </div>\n          <div class=\"col-6\">\n            <div class=\"text-tradeDetail \">\n              <div class=\"tradeDetail-name\">\n                <h2 class=\"tradeDetail-name-eng\">\n                  {{ main_item.name.en }}\n                </h2>\n                <h2>{{ main_item.name.th }}</h2>\n                <div class=\"tradeDetail-star\">\n                  <i class=\"fas fa-star\"></i>\n                  <i class=\"fas fa-star\"></i>\n                  <i class=\"fas fa-star\"></i>\n                  <i class=\"fas fa-star\"></i>\n                  <i class=\"fas fa-star\"></i>\n                </div>\n              </div>\n              <div class=\"tradeDetail-price\">\n                <h3>price</h3>\n                <h2>{{ main_item.amount | number_format }} ฿</h2>\n                <!--<h6>(100 instock)</h6>-->\n              </div>\n\n              <div class=\"tradeDetail-price row\">\n                <button class=\"btn-Trade btn\">Trade</button>\n              </div>\n            </div>\n          </div>\n        </div>\n        <div class=\"empty-space\"></div>\n        <div class=\"row\">\n          <div class=\"content-third-area container-allitem-sm row\">\n            <div class=\"content-third-itile col-12\">\n              <h2>ไอเท็ม อื่นๆ</h2>\n            </div>\n            <div class=\"content-third-item row\" id=\"Allitemgrd\">\n              <div\n                class=\"item-grid  col-3\"\n                :v-if=\"other_item.length > 0\"\n                v-for=\"(v, key) in other_item\"\n                :key=\"key\"\n              >\n                <img :src=\"v.picture\" class=\"item-img\" />\n                <div class=\"item-detail\">\n                  <h2>\n                    {{ v.name.en }}\n                  </h2>\n                  <h1>\n                    {{ v.name.th }}\n                  </h1>\n                </div>\n                <div class=\"row item-detail-price\">\n                  <p>{{ v.amount | number_format }}.-</p>\n                  <button class=\"btn\">Trade</button>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"empty-space\"></div>\n\n        <div class=\"content-third-area container-allitem-sm row\">\n          <div class=\"content-third-itile col-12\">\n            <h2\n              style=\"color: #111741;\n\"\n            >\n              ไอเท็มจากเกมส์เดียวกัน\n            </h2>\n          </div>\n          <div class=\"content-third-item row\" id=\"Allitemgrd\">\n            <div\n              class=\"item-grid  col-3\"\n              v-for=\"(v, key) in other_item\"\n              :key=\"key\"\n            >\n              <router-link\n                :to=\"{\n                  path: '/trade/' + v.game_id + '/' + v.id,\n                }\"\n              >\n                <img :src=\"v.picture\" class=\"item-img\" />\n                <div class=\"item-detail\">\n                  <h2 :class=\"v.class | color_sty\">\n                    {{ v.name.en }}\n                  </h2>\n                  <h1>{{ v.name.th }}</h1>\n                </div>\n              </router-link>\n              <div class=\"row item-detail-price\">\n                <p>{{ v.amount | number_format }}.-</p>\n                <router-link\n                  :to=\"{\n                    path: '/trade/' + v.game_id + '/' + v.id,\n                  }\"\n                  @click.native=\"loadOnce()\"\n                  class=\"btn\"\n                  >Trade</router-link\n                >\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"content-third-itile col-12\" style=\"\">\n          <h2\n            style=\"color: #111741;\n\"\n          >\n            ไอเท็มจากเกมส์เดียวกัน\n          </h2>\n        </div>\n        <div class=\"content-third-item\" id=\"Allitemswp\">\n          <div class=\"main-carousel\">\n            <div\n              class=\"carousel-cell carousel-style-trade carousel-style\"\n              v-for=\"(v, key) in other_item\"\n              :key=\"key\"\n            >\n              <router-link\n                :to=\"{\n                  path: '/trade/' + v.game_id + '/' + v.id,\n                }\"\n                @click.native=\"loadOnce()\"\n              >\n                <img :src=\"v.picture\" class=\"item-img\" />\n                <div class=\"overlay\">\n                  <div class=\"text\">{{v.name.th}}</div>\n                </div>\n              </router-link>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Flickity from \"flickity\";\n\nexport default {\n  data() {\n    return {\n      main_item: [],\n      Allitem: [],\n      other_item: [],\n      gameid: this.$route.params.gameid,\n      id: this.$route.params.id,\n    };\n  },\n\n  mounted() {\n    this.getData();\n    console.log(this.$route.params.id);\n  },\n\n  methods: {\n    loadOnce() {\n      location.reload();\n    },\n    getData() {\n      const self = this;\n      this.axios\n        .get(\n          this.$API_URL +\n            \"/api/v1/frontend/getdetailtrade/\" +\n            this.$route.params.gameid +\n            \"/\" +\n            this.$route.params.id\n        )\n        .then((response) => {\n          console.log(response.data.data.other_item);\n          this.main_item = response.data.data.main_item;\n          this.other_item = response.data.data.other_item;\n\n          this.$nextTick(function() {\n            self.flkty(); // DOM updated\n          });\n        });\n    },\n    flkty() {\n      var elem = document.querySelector(\".main-carousel\");\n      new Flickity(elem, {\n        // options\n        // groupCells: true,\n        cellAlign: \"left\",\n        contain: true,\n        groupCells: 5,\n        wrapAround: true,\n        prevNextButtons: false,\n        pageDots: false,\n        // draggable: false,\n        on: {\n          dragStart: () => {\n            elem.style.pointerEvents = \"none\";\n          },\n          dragEnd: () => {\n            elem.style.pointerEvents = \"all\";\n          },\n        },\n      });\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}