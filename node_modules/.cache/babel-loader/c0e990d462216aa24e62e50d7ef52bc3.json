AAA,CAAA,YAAA;AACA,YAAA,QAAA,CAAA,MAAA;AACA,WAFA,EAEA,IAFA,CAAA;AAGA,SALA,MAKA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,iBAAA,EAAA,OAAA;;AACA,UAAA,MAAA,CAAA,gBAAA;AACA;AACA,OAZA,EAaA,KAbA,CAaA,UAAA,KAAA,EAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,KAAA,CAAA,OAAA;;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,iBAAA,EAAA,OAAA;;AACA,QAAA,MAAA,CAAA,gBAAA;AACA,OAjBA;AAkBA,KAzLA;AA0LA,IAAA,gBA1LA,8BA0LA;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,UAAA;AACA,KA5LA;AA6LA,IAAA,aA7LA,yBA6LA,CA7LA,EA6LA;AACA,UAAA,KAAA,GAAA,IAAA,MAAA,CAAA,UAAA,CAAA;AACA,UAAA,GAAA,GAAA,MAAA,CAAA,YAAA,CAAA,CAAA,CAAA,CAAA,QAAA,GAAA,CAAA,CAAA,KAAA,GAAA,CAAA,CAAA,QAAA,CAAA;;AACA,UAAA,KAAA,CAAA,IAAA,CAAA,GAAA,CAAA,EAAA;AACA,eAAA,IAAA;AACA;;AAEA,MAAA,CAAA,CAAA,cAAA;AACA,aAAA,KAAA;AACA,KAtMA;AAuMA,IAAA,YAvMA,0BAuMA;AACA,WAAA,YAAA,GAAA,EAAA;AACA,UAAA,IAAA,GAAA,IAAA;AACA,MAAA,WAAA,CAAA,YAAA;AACA,YAAA,IAAA,CAAA,YAAA,IAAA,CAAA,EAAA;AACA,UAAA,IAAA,CAAA,YAAA,GAAA,IAAA,CAAA,YAAA,GAAA,CAAA;AACA;AACA,OAJA,EAIA,IAJA,CAAA;AAKA;AA/MA,GA/BA;AAgPA,EAAA,KAAA,EAAA;AACA,IAAA,cADA,4BACA;AACA,WAAA,QAAA,GAAA,KAAA,cAAA;;AACA,UAAA,KAAA,QAAA,IAAA,MAAA,IAAA,KAAA,QAAA,IAAA,SAAA,EAAA;AACA,eAAA,KAAA;AACA,OAFA,MAEA;AACA,aAAA,iBAAA;AACA;AACA,KARA;AASA,IAAA,QATA,sBASA;AACA,UAAA,KAAA,QAAA,KAAA,SAAA,EAAA;AACA,aAAA,YAAA,GAAA,mBAAA;AACA,aAAA,UAAA,GAAA,CAAA;AACA,aAAA,WAAA,GAAA,SAAA;AAGA,aAAA,WAAA,GAAA,EAAA;AACA,OAPA,MAOA,IAAA,KAAA,QAAA,KAAA,UAAA,EAAA;AACA,aAAA,YAAA,GAAA,mBAAA;AACA,aAAA,UAAA,GAAA,CAAA;AACA,aAAA,WAAA,GAAA,SAAA;AAGA,aAAA,WAAA,GAAA,EAAA;AACA,OAPA,MAOA,IAAA,KAAA,QAAA,KAAA,SAAA,EAAA;AACA,aAAA,UAAA,GAAA,CAAA;AACA,aAAA,YAAA,GAAA,oBAAA;AACA,aAAA,WAAA,GAAA,SAAA;AAEA,aAAA,WAAA,GAAA,GAAA;AACA,OANA,MAMA,IAAA,KAAA,QAAA,KAAA,MAAA,EAAA;AACA,aAAA,YAAA,GAAA,8CAAA;AACA,aAAA,UAAA,GAAA,CAAA;AACA,aAAA,WAAA,GAAA,CAAA;AAEA,aAAA,MAAA,CACA,wDADA,EAEA,+BAFA,EAGA,OAHA;AAKA,OAVA,MAUA;AACA,aAAA,UAAA,GAAA,CAAA;AACA;AACA;AA3CA,GAhPA,CA6RA;;AA7RA,CAAA","sourcesContent":["<template>\r\n  <div\r\n    class=\"modal  fade\"\r\n    id=\"ModaleditProfile\"\r\n    tabindex=\"-2\"\r\n    role=\"dialog\"\r\n    aria-labelledby=\"ModaleditProfileLabel\"\r\n    aria-hidden=\"true\"\r\n  >\r\n    <div class=\"modal-dialog \" role=\"document\">\r\n      <div\r\n        class=\"modal-content modal-content-editProfilemodal\"\r\n        id=\"input-bank\"\r\n        style=\"display:block\"\r\n      >\r\n        <div class=\"modal-header editProfilemodal-header\">\r\n          <button type=\"button\" class=\"close\"></button>\r\n          <h5 class=\"modal-title\" id=\"ModaleditProfileLabel\">\r\n            รายละเอียดบัญชี Wi Wallet\r\n          </h5>\r\n          <div class=\"close-editProfilemodal\">\r\n            <button\r\n              type=\"button\"\r\n              id=\"dismiss_modal\"\r\n              class=\"btn\"\r\n              data-dismiss=\"modal\"\r\n              aria-label=\"Close\"\r\n            >\r\n              <span aria-hidden=\"true\">&times;</span>\r\n            </button>\r\n          </div>\r\n        </div>\r\n        <div class=\"modal-body\">\r\n          <!--\r\n          <div class=\"form-group editProfile-form-group\">\r\n            <label for=\"Bank\">ธนาคาร</label>\r\n\r\n            <select\r\n              class=\"form-control\"\r\n              id=\"Bank\"\r\n              v-model=\"Bank\"\r\n              placeholder=\"เลขบัญชี\"\r\n            >\r\n              <option value=\"\" disabled selected>เลือกธนาคาร</option>\r\n              <option v-for=\"(v, key) in ListBank\" :key=\"key\" :value=\"key\">{{\r\n                v\r\n              }}</option>\r\n            </select>\r\n          </div>\r\n\r\n         -->\r\n          <div v-if=\"pagestatus === 0\">\r\n            <div class=\"form-group editProfile-form-group\">\r\n              <div class=\"alert\">\r\n                ** ข้อมูลจะต้องตรงกับ ข้อมูลที่ลงทะเบียนกับ Wi Wallet เท่านั้น\r\n                **\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"form-group editProfile-form-group\">\r\n              <label for=\"first_name\">ชื่อ(ที่ลงทะเบียนกับ Wi Wallet)</label>\r\n              <br />\r\n              <div class=\"gender-select\">\r\n                <div class=\"mr\">\r\n                  <input type=\"radio\" id=\"mr\" value=\"mr\" v-model=\"picked\" />\r\n                  <label for=\"mr\">นาย</label>\r\n                </div>\r\n                <div class=\"mrs\">\r\n                  <input type=\"radio\" id=\"mrs\" value=\"mrs\" v-model=\"picked\" />\r\n                  <label for=\"mrs\">นาง/นางสาว</label>\r\n                </div>\r\n              </div>\r\n              <input\r\n                type=\"text\"\r\n                class=\"form-control\"\r\n                id=\"first_name\"\r\n                placeholder=\"ชื่อ\"\r\n                v-model=\"first_name\"\r\n                maxlength=\"15\"\r\n              />\r\n            </div>\r\n\r\n            <div class=\"form-group editProfile-form-group\">\r\n              <label for=\"last_name\">นามสกุล(ที่ลงทะเบียนกับ Wi Wallet)</label>\r\n              <input\r\n                type=\"text\"\r\n                class=\"form-control\"\r\n                id=\"last_name\"\r\n                placeholder=\"นามสกุล\"\r\n                v-model=\"last_name\"\r\n              />\r\n            </div>\r\n\r\n            <div class=\"form-group editProfile-form-group\">\r\n              <label for=\"phone_number\"\r\n                >เบอร์โทรศัพท์ (ที่ลงทะเบียนกับ Wi Wallet)</label\r\n              >\r\n              <input\r\n                type=\"text\"\r\n                class=\"form-control\"\r\n                id=\"phone_number\"\r\n                placeholder=\"เบอร์โทรศัพท์\"\r\n                v-model=\"phone_number\"\r\n                @keypress=\"clsNumberOnly($event)\"\r\n                maxlength=\"10\"\r\n              />\r\n            </div>\r\n            <div class=\"form-group editProfile-form-group\">\r\n              <p>ถ้าคุณยังไม่มี Wi Wallet กรุณาโหลด Wi Wallet :</p>\r\n            </div>\r\n          </div>\r\n          <div v-else>\r\n            <div class=\"form-group editProfile-form-group\">\r\n              <p>ชื่อ : {{ first_name }}</p>\r\n            </div>\r\n            <div class=\"form-group editProfile-form-group\">\r\n              <p>นามสกุล : {{ last_name }}</p>\r\n            </div>\r\n            <div class=\"form-group editProfile-form-group\">\r\n              <p>เบอร์โทร : {{ phone_number }}</p>\r\n            </div>\r\n          </div>\r\n\r\n          <div\r\n            class=\"vertify-status\"\r\n            id=\"otp\"\r\n            v-if=\"pagestatus === 1 || pagestatus === 2\"\r\n          >\r\n            สถานะ :\r\n            <span :style=\"'color : ' + colorstatus + ';'\">\r\n              {{ wistatusText }}\r\n            </span>\r\n          </div>\r\n\r\n          <div class=\"vertify-input\" id=\"otp\" v-if=\"pagestatus === 3\">\r\n            <div class=\"otp-zone\">\r\n              <v-otp-input\r\n                ref=\"otpInput\"\r\n                input-classes=\"otp-input\"\r\n                separator=\"\"\r\n                :num-inputs=\"6\"\r\n                :should-auto-focus=\"true\"\r\n                :is-input-num=\"true\"\r\n                @on-complete=\"handleOnComplete\"\r\n                v-model=\"otp\"\r\n              />\r\n              <div class=\"Sendagain\">\r\n                <div class=\"Sendagaintext\">\r\n                  Didn't receive the SMS?\r\n                </div>\r\n                <div\r\n                  class=\"counttrue\"\r\n                  @click=\"sentOTP()\"\r\n                  v-if=\"otpCountDown == 0\"\r\n                >\r\n                  Send again\r\n                </div>\r\n                <div class=\"countfalse\" v-else>\r\n                  Send again ({{ otpCountDown }})\r\n                </div>\r\n              </div>\r\n              <span>Reference : {{ refno }}</span>\r\n              <!-- <a href=\"\">Enter different phone number</a> -->\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"modal-footer\">\r\n          <button\r\n            v-if=\"pagestatus === 3\"\r\n            type=\"button\"\r\n            class=\"btn btn-confirm-editProfile\"\r\n            autocomplete=\"off\"\r\n            @click=\"confirmOTP()\"\r\n          >\r\n            ยืนยัน OTP\r\n          </button>\r\n          <button\r\n            v-if=\"pagestatus === 2\"\r\n            type=\"button\"\r\n            class=\"btn btn-confirm-editProfile\"\r\n            autocomplete=\"off\"\r\n            @click=\"sentOTP()\"\r\n          >\r\n            ส่ง OTP\r\n          </button>\r\n\r\n          <button\r\n            v-if=\"pagestatus === 0\"\r\n            type=\"button\"\r\n            class=\"btn btn-confirm-editProfile\"\r\n            autocomplete=\"off\"\r\n            @click=\"sentWiWallet()\"\r\n          >\r\n            ยืนยัน\r\n          </button>\r\n          <div v-if=\"pagestatus === 1\" class=\"lodebarcontriner\">\r\n            <div class=\"lodebar\">\r\n              <div\r\n                class=\"lodebar-line\"\r\n                :style=\"'width:' + lodebarline + '%;'\"\r\n              ></div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Bank\",\r\n  props: [\"is_verify_otp\", \"is_verify_wiwallet\", \"wistatus_frist\"],\r\n\r\n  data() {\r\n    return {\r\n      selected: this.$selected ? this.$selected : 0,\r\n      image: \"@/assets/img/cardd.png\",\r\n      refno: \"\",\r\n      otp: \"\",\r\n      tokenOTP: \"\",\r\n      first_name: \"\",\r\n      last_name: \"\",\r\n      phone_number: \"\",\r\n      otpstatus: 0,\r\n      wistatus: \"\",\r\n      wistatusText: \"\",\r\n      picked: \"\",\r\n      password: \"\",\r\n      route: this.$route,\r\n      otpCountDown: 0,\r\n      lodebarline: 0,\r\n      pagestatus: 0,\r\n      colorstatus: '#000',\r\n    };\r\n  },\r\n\r\n  mounted() {\r\n    this.getnameWiWallet();\r\n  },\r\n\r\n  methods: {\r\n    getnameWiWallet() {\r\n      let config = {\r\n        headers: {\r\n          Authorization: `Bearer ${this.$cookies.get(\"token_main\")}`,\r\n        },\r\n      };\r\n\r\n      this.axios\r\n        .get(this.$API_URL + \"/api/v1/frontend/checkedwiwalletverify\", config)\r\n        .then((response) => {\r\n          // console.log(response.data);\r\n          this.first_name = response.data.registerdata.first_name;\r\n          this.last_name = response.data.registerdata.last_name;\r\n          this.phone_number = response.data.registerdata.phone;\r\n        })\r\n        .catch((error) => {\r\n          this.errorMessage = error.message;\r\n          this.wistatus = 0;\r\n        });\r\n    },\r\n\r\n    sentWiWallet() {\r\n      let t = this;\r\n      this.pagestatus = 1;\r\n      if (this.first_name && this.last_name && this.phone_number) {\r\n        if (this.phone_number != \"\") {\r\n          if (this.phone_number.trim().length >= 10) {\r\n            let body = {\r\n              phone: this.phone_number.trim(),\r\n              first_name: this.first_name.trim(),\r\n              last_name: this.last_name.trim(),\r\n            };\r\n            let config = {\r\n              headers: {\r\n                Authorization: `Bearer ${this.$cookies.get(\"token_main\")}`,\r\n              },\r\n            };\r\n\r\n            this.axios\r\n              .post(this.$API_URL + \"/api/v1/verify/wiwallet\", body, config)\r\n              .then((response) => {\r\n                if (response.data.code === 0) {\r\n                  this.StatusWiCountDown();\r\n\r\n                  // this.sentOTP();\r\n                } else {\r\n                  setTimeout(() => {\r\n                    t.$alert(\r\n                      \"โปรดตรวจสอบความถูกต้อง หรือ ติดต่อ Admin\",\r\n                      \"ไม่สามารถยืนยัน Wi Wallet ได้\",\r\n                      \"error\"\r\n                    );\r\n                  }, 2000);\r\n                }\r\n              })\r\n              .catch((error) => {\r\n                this.errorMessage = error.message;\r\n                setTimeout(() => {\r\n                  t.$alert(\r\n                    \"โปรดตรวจสอบความถูกต้อง หรือ ติดต่อ Admin\",\r\n                    \"ไม่สามารถยืนยัน Wi Wallet ได้\",\r\n                    \"error\"\r\n                  );\r\n                }, 2000);\r\n              });\r\n          }\r\n        } else {\r\n          this.$alert(\"กรุณากรอกข้อมูลให้ครบถ้วน\", \"เกิดข้อผิดพลาด\", \"error\");\r\n        }\r\n      } else {\r\n        this.$alert(\"กรุณากรอกข้อมูลให้ครบถ้วน\", \"เกิดข้อผิดพลาด\", \"error\");\r\n      }\r\n    },\r\n\r\n    checkStatusWi() {\r\n      let config = {\r\n        headers: {\r\n          Authorization: `Bearer ${this.$cookies.get(\"token_main\")}`,\r\n        },\r\n      };\r\n\r\n      this.axios\r\n        .get(this.$API_URL + \"/api/v1/frontend/checkedwiwalletverify\", config)\r\n        .then((response) => {\r\n          // console.log(response.data.status);\r\n\r\n          if (response.data.status === \"pending\") {\r\n            this.wistatus = \"pending\";\r\n          } else if (response.data.status === \"checking\") {\r\n            this.wistatus = \"checking\";\r\n          } else if (response.data.status === \"success\") {\r\n            this.wistatus = \"success\";\r\n          } else if (response.data.status === \"fail\") {\r\n            this.wistatus = \"fail\";\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          this.errorMessage = error.message;\r\n          // console.log(error);\r\n          this.wistatus = 0;\r\n          this.$alert(\r\n            \"โปรดตรวจสอบความถูกต้อง หรือ ติดต่อ Admin\",\r\n            \"ไม่สามารถยืนยัน Wi Wallet ได้\",\r\n            \"error\"\r\n          );\r\n        });\r\n    },\r\n    StatusWiCountDown() {\r\n      this.checkStatusWi();\r\n\r\n      let self = this;\r\n      this.$nextTick(function() {\r\n        // console.log(\"1\");\r\n        const Inter = setInterval(function() {\r\n          if (self.wistatus != \"fail\" || self.wistatus != \"success\") {\r\n            self.checkStatusWi();\r\n          } else {\r\n            clearInterval(Inter);\r\n            return false;\r\n          }\r\n        }, 10000);\r\n      });\r\n    },\r\n\r\n    sentOTP() {\r\n      this.pagestatus = 3;\r\n      this.CountDownotp();\r\n\r\n      let body = {\r\n        phone: this.phone_number,\r\n      };\r\n      let config = {\r\n        headers: {\r\n          Authorization: `Bearer ${this.$cookies.get(\"token_main\")}`,\r\n        },\r\n      };\r\n\r\n      this.axios\r\n        .post(this.$API_URL + \"/api/v1/otp/send-request\", body, config)\r\n        .then((response) => {\r\n          // console.log(response);\r\n          if (response.data.code === 0) {\r\n            this.refno = response.data.result.refno;\r\n            this.tokenOTP = response.data.result.token;\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    handleOnComplete(value) {\r\n      this.otp = value;\r\n      this.confirmOTP();\r\n    },\r\n    confirmOTP() {\r\n      let body = {\r\n        token: this.tokenOTP,\r\n        pin: this.otp,\r\n      };\r\n      let config = {\r\n        headers: {\r\n          Authorization: `Bearer ${this.$cookies.get(\"token_main\")}`,\r\n        },\r\n      };\r\n\r\n      this.axios\r\n        .post(this.$API_URL + \"/api/v1/otp/send-verify\", body, config)\r\n        .then((response) => {\r\n          if (response.data.code === 0) {\r\n            this.$alert(\"\", \"ยืนยัน Wi Wallet สำเร็จ\", \"success\");\r\n            setTimeout(() => {\r\n              location.reload();\r\n            }, 2000);\r\n          } else {\r\n            this.$alert(\"\", \"OTP  ไม่ถูกต้อง\", \"error\");\r\n            this.handleClearInput();\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          this.errorMessage = error.message;\r\n          this.$alert(\"\", \"OTP  ไม่ถูกต้อง\", \"error\");\r\n          this.handleClearInput();\r\n        });\r\n    },\r\n    handleClearInput() {\r\n      this.$refs.otpInput.clearInput();\r\n    },\r\n    clsNumberOnly(e) {\r\n      var regex = new RegExp(\"^[0-9]+$\");\r\n      var str = String.fromCharCode(!e.charCode ? e.which : e.charCode);\r\n      if (regex.test(str)) {\r\n        return true;\r\n      }\r\n\r\n      e.preventDefault();\r\n      return false;\r\n    },\r\n    CountDownotp() {\r\n      this.otpCountDown = 10;\r\n      let self = this;\r\n      setInterval(function() {\r\n        if (self.otpCountDown != 0) {\r\n          self.otpCountDown = self.otpCountDown - 1;\r\n        }\r\n      }, 1000);\r\n    },\r\n  },\r\n  watch: {\r\n    wistatus_frist() {\r\n      this.wistatus = this.wistatus_frist;\r\n      if (this.wistatus == \"fail\" || this.wistatus == \"success\") {\r\n        return false;\r\n      } else {\r\n        this.StatusWiCountDown();\r\n      }\r\n    },\r\n    wistatus() {\r\n      if (this.wistatus === \"pending\") {\r\n        this.wistatusText = \"กำลังรอการตรวจสอบ\";\r\n        this.pagestatus = 1;\r\n        this.colorstatus = '#eb9334';\r\n\r\n\r\n        this.lodebarline = 50;\r\n      } else if (this.wistatus === \"checking\") {\r\n        this.wistatusText = \"กำลังตรวจสอบสถานะ\";\r\n        this.pagestatus = 1;\r\n        this.colorstatus = '#FFE107';\r\n\r\n\r\n        this.lodebarline = 75;\r\n      } else if (this.wistatus === \"success\") {\r\n        this.pagestatus = 2;\r\n        this.wistatusText = \"ตรวจสอบสถานะสำเร็จ\";\r\n        this.colorstatus = '#54b325';\r\n\r\n        this.lodebarline = 100;\r\n      } else if (this.wistatus === \"fail\") {\r\n        this.wistatusText = \"ตรวจสอบสถานะไม่สำเร็จ โปรดตรวจสอบความถูกต้อง\";\r\n        this.pagestatus = 0;\r\n        this.lodebarline = 0;\r\n\r\n        this.$alert(\r\n          \"โปรดตรวจสอบความถูกต้อง หรือ สมัครแอปพลิเคชัน Wi Wallet\",\r\n          \"ไม่สามารถยืนยัน Wi Wallet ได้\",\r\n          \"error\"\r\n        );\r\n      } else {\r\n        this.pagestatus = 0;\r\n      }\r\n    },\r\n  },\r\n  // components: { History },\r\n};\r\n</script>\r\n\r\n<style scoped></style>\r\n"],"sourceRoot":"src/components"}]}