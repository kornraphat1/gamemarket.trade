,gBAAA;AACA,WAnBA;AAoBA;AACA;AACA,KArEA;AAuEA,IAAA,SAvEA,uBAuEA;AAAA;;AACA,UAAA,IAAA,GAAA,IAAA;;AACA,UAAA,KAAA,IAAA,IAAA,KAAA,OAAA,EAAA;AACA,YAAA,KAAA,IAAA,KAAA,KAAA,OAAA,EAAA;AACA,eAAA,QAAA,CAAA,wBAAA,EAAA,IAAA,CAAA,YAAA;AACA,gBAAA,IAAA,GAAA;AACA,cAAA,KAAA,EAAA,MAAA,CAAA,SADA;AAEA,cAAA,QAAA,EAAA,MAAA,CAAA,IAFA;AAGA,cAAA,gBAAA,EAAA,MAAA,CAAA;AAHA,aAAA;AAKA,gBAAA,MAAA,GAAA;AACA,cAAA,OAAA,EAAA;AADA,aAAA;;AAIA,YAAA,MAAA,CAAA,KAAA,CACA,IADA,CAEA,MAAA,CAAA,QAAA,GAAA,mCAFA,EAGA,IAHA,EAIA,MAJA,EAMA,IANA,CAMA,UAAA,QAAA,EAAA;AACA,kBAAA,QAAA,CAAA,IAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AACA,gBAAA,IAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,kBAAA,IAAA,EAAA;AADA,iBAAA;AAGA,eAJA,MAIA;AACA,gBAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA,GAAA,CAAA,EAAA,EAAA,gBAAA,EAAA,OAAA;AACA;AACA,aAdA,EAeA,KAfA,CAeA,UAAA,KAAA,EAAA;AACA,cAAA,MAAA,CAAA,YAAA,GAAA,KAAA,CAAA,OAAA;;AACA,cAAA,MAAA,CAAA,MAAA,CACA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,GAAA,CAAA,EADA,EAEA,gBAFA,EAGA,OAHA;;AAKA,cAAA,IAAA,CAAA,gBAAA;AACA,aAvBA;AAwBA,WAlCA;AAmCA,SApCA,MAoCA;AACA,eAAA,MAAA,CACA,yCADA,EAEA,gBAFA,EAGA,OAHA;AAKA;AACA;AACA,KAtHA;AAwHA,IAAA,gBAxHA,4BAwHA,KAxHA,EAwHA;AACA,WAAA,GAAA,GAAA,KAAA;AACA,KA1HA;AA2HA,IAAA,gBA3HA,8BA2HA;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,UAAA;AACA,KA7HA;AA8HA,IAAA,gBA9HA,8BA8HA;AACA,WAAA,iBAAA,GACA,KAAA,iBAAA,KAAA,UAAA,GAAA,MAAA,GAAA,UADA;AAEA;AAjIA,GA9CA;AAiLA,EAAA,KAAA,EAAA,EAjLA;AAkLA,EAAA,QAAA,EAAA;AAlLA,CAAA","sourcesContent":["<template>\n  <div>\n    <div class=\"main-vertify-container\">\n      <div class=\"main-vertify\">\n        <div class=\"vertify-content\" id=\"vertify-input\">\n          <div class=\"vertify-header\">\n            <span>ยืนยันตัวตน</span>\n          </div>\n\n          <div class=\"vertify-nav row\">\n            <div\n              id=\"title-tel\"\n              class=\"col-4 vertify-item \"\n              :class=\"step == 1 ? 'op1' : ''\"\n            >\n              <div class=\"number-nav\">\n                <span>1</span>\n              </div>\n\n              <p>เบอร์</p>\n              <p>โทรศัพท์</p>\n            </div>\n\n            <div\n              id=\"title-otp\"\n              class=\"col-4 vertify-item \"\n              :class=\"step == 2 ? 'op1' : ''\"\n            >\n              <div class=\"number-nav\">\n                <span>2</span>\n              </div>\n\n              <p>ยืนยัน</p>\n              <p>OTP</p>\n            </div>\n\n            <div\n              id=\"title-pass\"\n              class=\"col-4 vertify-item \"\n              :class=\"step == 3 ? 'op1' : ''\"\n            >\n              <div class=\"number-nav\">\n                <span>3</span>\n              </div>\n\n              <p>สร้าง</p>\n              <p>รหัสผ่าน</p>\n            </div>\n          </div>\n\n          <div class=\"vertify-input\" v-if=\"step == 1\">\n            <input\n              class=\"inputTel\"\n              type=\"text\"\n              placeholder=\"กรุณาใส่เบอร์โทรศัพท์\"\n              v-model=\"tel\"\n              @keypress=\"clsNumberOnly($event)\"\n            />\n            <div class=\"vertify-button\">\n              <button v-on:click=\"telnext()\">ถัดไป</button>\n            </div>\n          </div>\n          <div class=\"vertify-input\" v-else-if=\"2\">\n            <div class=\"otp-zone\">\n              <v-otp-input\n                ref=\"otpInput\"\n                input-classes=\"otp-input\"\n                separator=\"\"\n                :num-inputs=\"6\"\n                :should-auto-focus=\"true\"\n                :is-input-num=\"true\"\n                @on-complete=\"handleOnComplete\"\n                v-model=\"otp\"\n              />\n\n              <p>\n                Didn't receive the SMS?\n                <span @click=\"Sendagain()\"> Send again</span>\n              </p>\n              <span>Reference : {{ refno }}</span>\n              <!-- <a href=\"\">Enter different phone number</a> -->\n            </div>\n            <div class=\"vertify-button\">\n              <button v-on:click=\"otpnext()\">ถัดไป</button>\n            </div>\n          </div>\n\n          <div class=\"vertify-input\" v-else-if=\"3\">\n            <div>\n              <vuePassword\n                :type=\"passwordFieldType\"\n                placeholder=\"สร้างรหัสผ่าน\"\n                :disableStrength=\"true\"\n                autocomplete=\"off\"\n                v-model=\"pass\"\n              />\n\n              <vuePassword\n                :type=\"passwordFieldType\"\n                placeholder=\"ยืนยันรหัสผ่าน\"\n                :disableStrength=\"true\"\n                autocomplete=\"off\"\n                v-model=\"passcon\"\n              />\n            </div>\n            <div class=\"alert_pass\">\n              <p>\n                **รหัสผ่านจะต้องประกอบไปด้วยตัวอักษร\n              </p>\n              <p>พิมพ์เล็ก,พิมพ์ใหญ่,และอักขระพิเศษ</p>\n            </div>\n            <div class=\"vertify-button\">\n              <button v-on:click=\"PassWnext()\">ถัดไป</button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      tel: \"\",\n      otp: \"\",\n      pass: \"\",\n      passcon: \"\",\n      verify_link: \"\",\n      token: \"\",\n      token_otp: \"\",\n      refno: \"\",\n      phone: \"\",\n      passwordFieldType: \"password\",\n      step: 1,\n    };\n  },\n\n  mounted() {\n    document.getElementById(\"footer-contener\").style.display = \"none\";\n\n    // let navbar = document.getElementById(\"navbar\");\n    // if (navbar) {\n    //   navbar.style.display = \"none\";\n    // }\n\n    this.verify_link = this.$route.query.verify_link;\n    this.axios\n      .get(\n        this.$API_URL +\n          \"/api/v1/otp/get-response/\" +\n          this.$route.query.verify_link\n      )\n      .then((response) => {\n        this.token_otp = response.data.result.token;\n        this.phone = response.data.result.phone;\n        this.refno = response.data.result.refno;\n      })\n      .catch((error) => {\n        // const self = this;\n        this.errorMessage = error.message;\n        if (error.response.data.code === 1421) {\n          this.$alert(error.response.data.msg.th, \"เกิดข้อผิดพลาด\", \"error\");\n        }\n      });\n  },\n\n  methods: {\n    clsNumberOnly(e) {\n      var regex = new RegExp(\"^[0-9]+$\");\n      var str = String.fromCharCode(!e.charCode ? e.which : e.charCode);\n      if (regex.test(str)) {\n        return true;\n      }\n\n      e.preventDefault();\n      return false;\n    },\n    telnext() {\n      this.step = 2;\n    },\n\n\n\n\n    Sendagain() {\n      const self = this;\n\n      let Sendagain_body = { token: this.token_otp, phone: this.phone };\n\n      this.axios\n        .post(this.$API_URL + \"/api/v1/otp/resend\", Sendagain_body)\n        .then((response) => {\n          self.$router.push({\n            name: \"Vertify\",\n            query: { verify_link: response.data.res.unique_id },\n          });\n        })\n        .catch((error) => {\n          this.errorMessage = error.message;\n\n          this.$alert(error.response.data.msg.th, \"เกิดข้อผิดพลาด\", \"error\");\n        });\n    },\n\n    otpnext() {\n      const self = this;\n      if (this.otp) {\n        if (this.token_otp) {\n          let body = { token: this.token_otp, pin: this.otp };\n          let config = {\n            headers: { Authorization: `Bearer ${this.token}` },\n          };\n\n          this.axios\n            .post(this.$API_URL + \"/api/v1/otp/send-verify\", body, config)\n            .then((response) => {\n              if (response.data.msg == \"success\") {\n                if (response.data.result.register_success == true) {\n                  self.$router.push({\n                    name: \"Login\",\n                  });\n                }\n              }\n            })\n            .catch((error) => {\n              this.errorMessage = error.message;\n              this.$alert(\n                error.response.data.msg.th,\n                \"เกิดข้อผิดพลาด\",\n                \"error\"\n              );\n              self.handleClearInput();\n            });\n        }\n      }\n    },\n\n    PassWnext() {\n      const self = this;\n      if (this.pass && this.passcon) {\n        if (this.pass === this.passcon) {\n          this.$confirm(\"ยืนยัน การตั้งรหัสผ่าน\").then(() => {\n            let body = {\n              token: this.token_otp,\n              password: this.pass,\n              password_confirm: this.passcon,\n            };\n            let config = {\n              headers: {},\n            };\n\n            this.axios\n              .post(\n                this.$API_URL + \"/api/v1/otp/game-member-new-regis\",\n                body,\n                config\n              )\n              .then((response) => {\n                if (response.data.code == 0) {\n                  self.$router.push({\n                    name: \"Login\",\n                  });\n                } else {\n                  this.$alert(response.data.msg.th, \"เกิดข้อผิดพลาด\", \"error\");\n                }\n              })\n              .catch((error) => {\n                this.errorMessage = error.message;\n                this.$alert(\n                  error.response.data.msg.th,\n                  \"เกิดข้อผิดพลาด\",\n                  \"error\"\n                );\n                self.handleClearInput();\n              });\n          });\n        } else {\n          this.$alert(\n            \"รหัสผ่านไม่ตรงกัน กรุณากรอกรหัสผ่านใหม่\",\n            \"เกิดข้อผิดพลาด\",\n            \"error\"\n          );\n        }\n      }\n    },\n\n    handleOnComplete(value) {\n      this.otp = value;\n    },\n    handleClearInput() {\n      this.$refs.otpInput.clearInput();\n    },\n    switchVisibility() {\n      this.passwordFieldType =\n        this.passwordFieldType === \"password\" ? \"text\" : \"password\";\n    },\n  },\n  watch: {},\n  computed: {},\n};\n</script>\n"],"sourceRoot":"src/components/LoginRegister"}]}