his.$API_URL + \"/api/v1/frontend/member/withdraw\",\r\n              body,\r\n              config\r\n            )\r\n            .then((response) => {\r\n              console.log(response);\r\n              // this.Allitem = response.data.data.list;\r\n            })\r\n            .catch((error) => {\r\n              this.errorMessage = error.message;\r\n              console.log(error);\r\n              // alert(error);\r\n            });\r\n        } else {\r\n          alert(\"กรุณาถอนขั้นต่ำ 100 บาท\");\r\n        }\r\n      }\r\n    },\r\n\r\n    logout() {\r\n      const self = this;\r\n\r\n      document.cookie = \"token_main=\";\r\n\r\n      this.$nextTick(function() {\r\n        self.$router.push({\r\n          name: \"Login\",\r\n        });\r\n      });\r\n    },\r\n  },\r\n  watch: {\r\n    withdrawnum() {\r\n      if (this.userdetail.balance < this.withdrawnum) {\r\n        alert(\"เงินของคุณไม่เพืยงพอ\");\r\n        this.withdrawnum = this.userdetail.balance;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped></style>\r\n"]}]}