     <div class=\"row textInCrad\">\r\n                  <div class=\"col-12\" style=\"\">\r\n                    <h4><i class=\"fas fa-wallet\"></i> Wallet</h4>\r\n                  </div>\r\n                  <div class=\"col-12 \">\r\n                    <div class=\"text-right\">\r\n                      <h3>{{ userdetail.balance | number_format }}.00</h3>\r\n                    </div>\r\n                  </div>\r\n\r\n                  <div class=\"col-4 \" style=\"position: absolute;bottom: 0px;\">\r\n                    <span class=\"c-holder\">Card holder</span>\r\n                    <h6 class=\"c-number\">\r\n                      {{ this.$cookies.get(\"phone_number\") }}\r\n                    </h6>\r\n                  </div>\r\n                </div>\r\n                <img src=\"@/assets/img/cardd.png\" alt=\"\" style=\" width:100%\" />\r\n              </div>\r\n            </div>\r\n            <div class=\"row\">\r\n              <div class=\"q-col-3\">\r\n                <a href=\"#\" @click=\"withdrawAdd(100)\" class=\"q-withdraw\">100</a>\r\n              </div>\r\n              <div class=\"q-col-3\">\r\n                <a href=\"#\" @click=\"withdrawAdd(300)\" class=\"q-withdraw\">300</a>\r\n              </div>\r\n              <div class=\"q-col-3\">\r\n                <a href=\"#\" @click=\"withdrawAdd(500)\" class=\"q-withdraw\">500</a>\r\n              </div>\r\n              <div class=\"q-col-3\">\r\n                <a href=\"#\" @click=\"withdrawAdd(1000)\" class=\"q-withdraw\"\r\n                  >1,000</a\r\n                >\r\n              </div>\r\n            </div>\r\n            <div class=\"row pd-top\">\r\n              <div class=\"col-7\">\r\n                <input\r\n                  type=\"text\"\r\n                  v-model=\"withdrawnum\"\r\n                  class=\"input-withdraw\"\r\n                  placeholder=\"00.0 ฿\"\r\n                />\r\n              </div>\r\n              <div class=\"col-4 p-0\">\r\n                <button class=\"btn btn-withdraw\" @click=\"withdraw()\">\r\n                  ถอนเงิน\r\n                </button>\r\n              </div>\r\n              <div class=\"col-12\">\r\n                <div class=\"withdraw-minimum-text\">\r\n                  <p>ถอนเงินขั้นต่ำ 100.00 บาท</p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"modal-footer\">\r\n            <button\r\n              class=\"btn btn-logout\"\r\n              data-dismiss=\"modal\"\r\n              @click=\"logout()\"\r\n            >\r\n              Logout\r\n            </button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"row navbar navbar-expand-lg navbar-light\" id=\"navbar\">\r\n      <nav class=\"row col-9 \" style=\"\">\r\n        <div class=\"menu-bar\">\r\n          <button\r\n            class=\"navbar-tog\"\r\n            type=\"button\"\r\n            data-toggle=\"collapse\"\r\n            data-target=\"#navbarNavDropdown\"\r\n            aria-controls=\"navbarNavDropdown\"\r\n            aria-expanded=\"false\"\r\n            aria-label=\"Toggle navigation\"\r\n            @onclick=\"hamberBar(this)\"\r\n          >\r\n            <div class=\"bar1\"></div>\r\n            <div class=\"bar2\"></div>\r\n            <div class=\"bar3\"></div>\r\n          </button>\r\n\r\n          <a class=\"navbar-brand\" href=\"/\">\r\n            <img\r\n              src=\"@/assets/img/logo.png\"\r\n              class=\"d-inline-block align-top logo\"\r\n              alt=\"\"\r\n          /></a>\r\n        </div>\r\n\r\n        <div class=\"collapse navbar-collapse navbar-menu-div\">\r\n          <ul class=\"navbar-nav nav-list-menu\">\r\n            <li class=\"nav-item\">\r\n              <router-link\r\n                to=\"/\"\r\n                :class=\"{ selected: selected === 0 }\"\r\n                @click.native=\"changeSelected(0)\"\r\n           