      v-model=\"searchQuery\"\n                placeholder=\"Search Product\"\n              />\n            </div>\n          </div>\n          <div class=\"row col-6 col-xs-6 col-sm-6 col-lg-5\">\n            <div class=\"col-6\">\n              <div class=\"dropdown input-group-prepend marketplace-fillter\">\n                <a\n                  class=\"btn marketplace-fillter-title  dropdown-toggle\"\n                  href=\"#\"\n                  role=\"button\"\n                  id=\"dropdownMenuLink\"\n                  data-toggle=\"dropdown\"\n                  aria-haspopup=\"true\"\n                  aria-expanded=\"false\"\n                >\n                  All Game\n                </a>\n\n                <div\n                  class=\"dropdown-menu marketplace-fillter-dropdown\"\n                  aria-labelledby=\"dropdownMenuLink\"\n                >\n                  <a class=\"dropdown-item\" href=\"#\">Action</a>\n                  <a class=\"dropdown-item\" href=\"#\">Another action</a>\n                  <a class=\"dropdown-item\" href=\"#\">Something else here</a>\n                </div>\n              </div>\n            </div>\n            <div class=\"col-6\">\n              <div class=\"dropdown input-group-prepend marketplace-fillter\">\n                <a\n                  class=\"btn marketplace-fillter-title  dropdown-toggle\"\n                  href=\"#\"\n                  role=\"button\"\n                  id=\"dropdownMenuLink\"\n                  data-toggle=\"dropdown\"\n                  aria-haspopup=\"true\"\n                  aria-expanded=\"false\"\n                >\n                  Sort By\n                </a>\n\n                <div\n                  class=\"dropdown-menu marketplace-fillter-dropdown\"\n                  aria-labelledby=\"dropdownMenuLink\"\n                >\n                  <a\n                    class=\"dropdown-item\"\n                    @click=\"sortvalue('price', 'asc')\"\n                    href=\"#\"\n                    >price ^</a\n                  >\n                  <a\n                    class=\"dropdown-item\"\n                    @click=\"sortvalue('price', 'desc')\"\n                    href=\"#\"\n                    >price</a\n                  >\n                  <a class=\"dropdown-item\" href=\"#\">Another action</a>\n                  <a class=\"dropdown-item\" href=\"#\">Something else here</a>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n        <div class=\"content-third-area row\">\n          <div class=\"content-third-item row\" id=\"itemGoodPrice\">\n            <div\n              class=\"item-grid col-3\"\n              v-for=\"(v, key) in resultQuery\"\n              :key=\"key\"\n            >\n              <router-link\n                class=\"static\"\n                :to=\"{\n                  path: '/trade',\n                  query: { id: v.class },\n                }\"\n              >\n                <img :src=\"v.img\" class=\"item-img\" />\n                <div class=\"item-detail\">\n                  <h2 :class=\"v.class | color_sty\">\n                    {{ v.name_eng }}\n                  </h2>\n                  <h1>{{ v.name_thai }}</h1>\n                </div>\n                <div class=\"row item-detail-price\">\n                  <p>{{ v.price | number_format }}.-</p>\n                  <button class=\"btn\">Trade</button>\n                </div>\n              </router-link>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      Allitem: [],\n      searchQuery: null,\n      sortBy: null,\n      sortDirection: null,\n    };\n  },\n\n  mounted() {\n    this.getData();\n  },\n\n  methods: {\n    getData() {\n      this.axios\n        .get(\n          \"http://api-gamemarget.ap-southeast-1.elasticbeanstalk.com/api/v1/item/getall\"\n        )\n        .then((response) => {\n          this.Allitem = response.data.itemAlls;\n        });\n    },\n    sortvalue(i, d) {\n      console.log(i);\n      this.sortBy = i;\n      this.sortDirection = d;\n    },\n  },\n  computed: {\n    resultQuery() {\n      if (this.searchQuery) {\n        return this.Allitem.filter((item) => {\n          return this.searchQuery\n            .toLowerCase()\n            .split(\" \")\n            .every((v) =>\n              (item.name_eng + item.name_thai + item.price)\n                .toLowerCase()\n                .includes(v)\n            );\n        });\n      } else {\n        return this.Allitem;\n      }\n    },\n  },\n};\n</script>\n"]}]}