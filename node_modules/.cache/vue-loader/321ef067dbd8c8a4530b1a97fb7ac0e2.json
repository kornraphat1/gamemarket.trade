ock\"\r\n      >\r\n        <div class=\"modal-header Historymodal-header\">\r\n          <h5 class=\"modal-title\" id=\"ModalHistoryLabel\">\r\n            History Trade / Withdraw\r\n          </h5>\r\n        </div>\r\n\r\n        <div class=\"modal-body\">\r\n          <div class=\"row history-header\">\r\n            <div class=\"input-group col-12\">\r\n              <div class=\"input-group-prepend\">\r\n                <span class=\"input-group-text\" id=\"basic-addon1\"\r\n                  ><i class=\"fas fa-search\"></i\r\n                ></span>\r\n              </div>\r\n              <input\r\n                type=\"text\"\r\n                class=\"form-control\"\r\n                placeholder=\"Search\"\r\n                aria-label=\"Search\"\r\n                aria-describedby=\"basic-addon1\"\r\n              />\r\n            </div>\r\n          </div>\r\n          <div class=\"row history-header\">\r\n            <div class=\"history-fillter-group\">\r\n              <div class=\" history-fillter\">\r\n                <Select2\r\n                  :settings=\"{\r\n                    minimumResultsForSearch: Infinity,\r\n                    placeholder: 'Sort by Game',\r\n                  }\"\r\n                />\r\n              </div>\r\n              <div class=\" history-fillter\">\r\n                <Select2\r\n                  :settings=\"{\r\n                    minimumResultsForSearch: Infinity,\r\n                    placeholder: 'Sort by Type',\r\n                  }\"\r\n                />\r\n              </div>\r\n            </div>\r\n            <div class=\"history-fillter-group\">\r\n              <div style=\"margin: auto 5px;\"><span>Date:</span></div>\r\n\r\n              <div class=\" history-fillter\">\r\n                <Select2\r\n                  :settings=\"{\r\n                    minimumResultsForSearch: Infinity,\r\n                    placeholder: 'Last Date',\r\n                  }\"\r\n                />\r\n              </div>\r\n              <div  style=\"margin: auto 5px;\"><span>TO</span></div>\r\n              <div class=\" history-fillter\">\r\n                <Select2\r\n                  :settings=\"{\r\n                    minimumResultsForSearch: Infinity,\r\n                    placeholder: 'Last Date',\r\n                  }\"\r\n                />\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <vue-good-table\r\n            :columns=\"columns\"\r\n            :rows=\"rows\"\r\n            :line-numbers=\"true\"\r\n            max-height=\"65vh\"\r\n          >\r\n            <template slot=\"table-row\" slot-scope=\"props\">\r\n              <span v-if=\"props.row.type == 'withdraw'\">\r\n                <span style=\" color: blue; \">{{\r\n                  props.formattedRow[props.column.field]\r\n                }}</span>\r\n              </span>\r\n              <span v-else>\r\n                {{ props.formattedRow[props.column.field] }}\r\n              </span>\r\n            </template>\r\n\r\n            <div slot=\"emptystate\">\r\n              This will show up when there are no rows\r\n            </div></vue-good-table\r\n          >\r\n        </div>\r\n\r\n        <!-- <div class=\"modal-footer\">\r\n          <button type=\"button\" class=\"btn btn-confirm-editProfile\">\r\n            ยืนยัน\r\n          </button>\r\n        </div> -->\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n  name: \"History\",\r\n  props: {\r\n    msg: String,\r\n  },\r\n\r\n  data() {\r\n    return { currentDate:'',\r\n      body: {\r\n        perpage: 100,\r\n       \r\n        page: 1,\r\n        filter: {\r\n          // search: \"\",\r\n          // game_id: 1,\r\n          // type: \"withdraw\",\r\n        },\r\n        start_date: \"2021-11-01\",\r\n        end_date: \"2021-11-30\",\r\n      },\r\n      columns: [\r\n        {\r\n          label: \"Game Name\",\r\n          field: \"game_name\",\r\n          tdClass: this.tdClassFunc,\r\n          thClass: \"thClass \",\r\n        },\r\n        {\r\n          label: \"Item Name\",\r\n          field: \"item_name\",\r\n          tdClass: this.tdClassFunc,\r\n          thClass: \"thClass \",\r\n          type: \"text\",\r\n        },\r\n\r\n        {\r\n          label: \"Amount\",\r\n          field: \"amount\",\r\n          tdClass: this.tdClassFunc,\r\n          thClass: \"thClass \",\r\n          type: \"number\",\r\n        },\r\n        {\r\n          label: \"Date\",\r\n          field: \"created_at\",\r\n          tdClass: this.tdClassFunc,\r\n          thClass: \"thClass \",\r\n          type: \"text\",\r\n        },\r\n        {\r\n          label: \"Type\",\r\n          field: \"type\",\r\n          thClass: \"thClass \",\r\n          tdClass: this.tdClassFunc,\r\n          type: \"text\",\r\n        },\r\n      ],\r\n      rows2: [\r\n        { game_name: 1, name: \"John\", age: 20, createdAt: \"\", score: 0.03343 },\r\n        {\r\n          id: 2,\r\n          name: \"Jane\",\r\n          age: 24,\r\n          createdAt: \"2011-10-31\",\r\n          score: 0.03343,\r\n        },\r\n      ],\r\n      rows: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getData();\r\n    console.log(this.rows);\r\n  },\r\n\r\n  methods: {\r\n    getData() {\r\n      const config = {\r\n        headers: { Authorization: `Bearer ${this.$cookies.get(\"token_main\")}` },\r\n      };\r\n      // console.log(this.$cookies.get(\"token\"));\r\n      this.axios\r\n        .post(this.$API_URL + \"/api/v1/frontend/listhistory\", this.body, config)\r\n        .then((response) => {\r\n          console.log(response.data.data);\r\n          // this.rows = response.data.data.list;\r\n          response.data.data.list.forEach((element) => {\r\n            // console.log(element.item_name[\"en\"]);\r\n\r\n            this.rows.push({\r\n              game_name: element.game_name.en,\r\n              item_name: element.item_name.en,\r\n              amount: element.amount,\r\n              type: element.type,\r\n              created_at: element.created_at,\r\n            });\r\n          });\r\n        })\r\n        .catch((error) => {\r\n          this.errorMessage = error.message;\r\n          console.log(error);\r\n          // alert(error);\r\n        });\r\n    },\r\n\r\n    tdClassFunc(row) {\r\n      if (row.type == \"withdraw\") {\r\n        return \"bg-withdraw\";\r\n      }\r\n      return \"green-class\";\r\n    },\r\n  },\r\n  watch: {},\r\n  components: {},\r\n};\r\n</script>\r\n\r\n<style scoped></style>\r\n"]}]}